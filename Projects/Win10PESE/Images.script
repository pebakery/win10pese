[Best_Image_Index_Workaround_A_Loop]
//Mostly same section 'Images Configuration' and 'Extract Wim Folders' - Only difference %IndexInstall% %WIMIndex%
StrFormat,Dec,#c,%WIMImageCount%
StrFormat,Inc,#c,-1
StrFormat,Mult,#c,-1
IniRead,%ProjectTemp%\wiminfo\WimInfoInstall.ini,#c,EDITIONID,#9
StrFormat,Dec,#c,1
IniRead,%ProjectTemp%\wiminfo\WimInfoInstall.ini,#c,EDITIONID,#8
StrFormat,Right,#9,1,#7
If,#7,Equal,N,Begin
StrFormat,RTrim,#9,1,#7
If,#7,Equal,#8,Begin
StrFormat,Replace,#7,Workstation,-,#6
StrFormat,Replace,#6,Education,-,#6
If,#6,Equal,#7,Set,%WIMIndex%,#c
End
End
Else,Begin
If,Not,%WIMIndex%,Equal,-,Begin
StrFormat,Replace,#9,Workstation,-,#6
StrFormat,Replace,#6,Education,-,#6
If,#9,Equal,#7,Set,%WIMIndex%,#c
End
End
If,Not,%WIMIndex%,Equal,-,Loop,Break

