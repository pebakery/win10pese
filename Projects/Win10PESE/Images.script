//message,"UILanguages: %DistLang% - MuiLang: %MuiLangVar%"
//IniDeleteSection,%ProjectDir%\script.project,MuiLang
IniDelete,%ProjectDir%\script.project,MuiLang,#$pMuiLangAll#$p
IniDelete,%ProjectDir%\script.project,MuiLang,#$pMuiLang1#$p
IniDelete,%ProjectDir%\script.project,MuiLang,#$pMuiLang2#$p
IniDelete,%ProjectDir%\script.project,MuiLang,#$pMuiLang3#$p
IniDelete,%ProjectDir%\script.project,MuiLang,#$pMuiLang4#$p
StrFormat,RTrim,%MuiLangVar%,1,%t%
IniWrite,%ProjectDir%\script.project,MuiLang,#$pMuiLangAll#$p,%t%
