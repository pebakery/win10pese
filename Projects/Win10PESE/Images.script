[PrepPE]
//Tip: Check Gena PrepPE to get codes ready when-if required ;)
Call,HostUserProfileDir
StrFormat,Pos,%BaseDir%,%HostUserProfileDir%,%pos%
If,%pos%,Equal,1,Begin
Message,"WinPESE cannot run from inside a user's profile directory. #$x Try using Basic Folder. #$x Ex: D:\Oven\",Error
Halt,"WinPESE cannot run from inside a user's profile directory. Try using Basic Folder. Ex: D:\Oven\"
End
//-
System,IsAdmin,%t%
Echo,"Run as administrator=%t%"
//-
Call,HostOSVersion,,1
If,%HostOSVersion1%,Bigger,5,Begin
Set,%UAC%,"Value Not Found"
If,ExistRegKey,HKLM,SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System,EnableLUA,RegRead,HKLM,SOFTWARE\Microsoft\Windows\CurrentVersion\Policies\System,EnableLUA,%UAC%
//UAC Disabled 0
Echo,"UAC=%UAC%"
End
//-
Call,HostSessionName
Set,%t%,0
If,%HostSessionName%,Equal,Console,Set,%t%,1
If,%HostSessionName%-,Equal,-,Set,%t%,1
Echo,SessionName=%HostSessionName%
If,%t%,Equal,0,Echo,"Info: Builder run on Terminal Connection SessionName=%HostSessionName%",Warn
//
//Check 'Target Directory' Setting. TargetDir Setting is fixed if needed with the right path. It requires a restart of the builder to load it in memory.
//FAQ http://TheOven.org/index.php?topic=834 - My Build Fails (or BSOD or any other trouble) - What to do  ? - Be sure you DO NOT change 'Target Directory' and 'Iso File' settings
//- Allow custom TargetDir sub-folder. Probably more flexible (eg: Win10PESE_x64).
StrFormat,Split,%ProjectDir%,\,0,%t%
StrFormat,Split,%ProjectDir%,\,%t%,%ProjectFolderName%
If,Not,%TargetDir%,Equal,%BaseDir%\Target\%ProjectFolderName%,Begin
IniWrite,%ProjectDir%\script.project,Main,TargetDir,#$pBaseDir#$p\Target\%ProjectFolderName%
End
//If,Not,%TargetDir%,Equal,%BaseDir%\Target\%ProjectName%,Begi-n
//IniWrite,%ProjectDir%\script.project,Main,TargetDir,#$pBaseDir#$p\Target\%ProjectName%
//En-d
//--
//Check 'Iso File' Setting. IsoFile Setting is fixed if needed with the right path. It requires a restart of the builder to load it in memory.
//FAQ http://TheOven.org/index.php?topic=834 - My Build Fails (or BSOD or any other trouble) - What to do  ? - Be sure you DO NOT change 'Target Directory' and 'Iso File' settings
StrFormat,Path,%IsoFile%,%IsoFolder%
StrFormat,CTrim,%IsoFolder%,\,%IsoFolder%
//-
If,Not,%IsoFolder%,Equal,%BaseDir%\ISO,Begin
Set,%IsoFileTmp%,#$pBaseDir#$p\ISO\%ProjectName%.iso
If,%ProjectName%,Equal,Win7PESE,Set,%IsoFileTmp%,#$pBaseDir#$p\ISO\Win7PE.iso
If,%ProjectName%,Equal,Win8PESE,Set,%IsoFileTmp%,#$pBaseDir#$p\ISO\Win8PE.iso
//If,%ProjectName%,Equal,Win8.1SE,Set,%IsoFileTmp%,#$pBaseDir#$p\ISO\Win8.1SE.iso
If,%ProjectName%,Equal,Win10PESE,Set,%IsoFileTmp%,#$pBaseDir#$p\ISO\Win10PE.iso
IniWrite,%ProjectDir%\script.project,Main,IsoFile,%IsoFileTmp%
End
//--
// Second check 'Project Dir Folder Name', 'Target Directory' and 'Iso File' with message, halt to Restart the Builder.
//If,Not,%ProjectName%,Equal,%ProjectFolderName%,Call,Echo,"ERROR: The 'Project Dir Folder Name' %ProjectFolderName% path does not match the project name %ProjectName%.#$xDO NOT change the Project Dir Folder Name!#$x#$xClose the builder and rename the folder before Restarting this application. Halt",Warn,,MessageError,,Halt
//If,Not,%TargetDir%,Equal,%BaseDir%\Target\%ProjectName%,Call,Echo,"ERROR: The 'Target Directory' path does not match the project settings.#$x  Current: %TargetDir%#$x  Default: %BaseDir%\Target\%ProjectName#$xDO NOT change the'Target Directory' setting!%#$x#$xClose and Restart this Builder to get the right configuration. Halt",Warn,,MessageError,,Halt
If,Not,%TargetDir%,Equal,%BaseDir%\Target\%ProjectFolderName%,Call,Echo,"ERROR: The 'Target Directory' path does not match the project settings.#$x  Current: %TargetDir%#$x  Default: %BaseDir%\Target\%ProjectFolderName%#$xDO NOT change the'Target Directory' setting!#$x#$xClose and Restart this Builder to get the right configuration. Halt",Warn,,MessageError,,Halt
If,Not,%IsoFolder%,Equal,%BaseDir%\ISO,Call,Echo,"ERROR: The 'ISO File' path does not match the project settings.#$x  Current: %IsoFolder%#$x  Default: %BaseDir%\ISO#$xDO NOT change the 'ISO File' setting!#$x#$xClose and Restart this Builder to get the right configuration. Halt",Warn,,MessageError,,Halt
//------
Run,%ProjectDir%\Utils\HiveUnLoad.script,HiveUnloadNames
Run,%ProjectDir%\Utils\HiveUnLoad.script,HiveLoadTestProjectTempMessageHalt,%ProjectDir%\Utils\HiveUnLoad.script

[CheckHostOS]
// XP, 2k3, Vista Not Compatible with Win8.1SE
Set,%XP2k3HostOS%,No
Call,HostOSVersion,,4x
If,%HostOSVersion1%,Equal,5,Set,%XP2k3HostOS%,Yes
If,%HostOSVersion1%.%HostOSVersion2%,Equal,6.0,Set,%XP2k3HostOS%,Yes
// XP, 2k3, Vista Not Compatible with Win8.1SE, Win10PESE
If,%XP2k3HostOS%,Equal,Yes,Begin
If,%ProjectName%,Equal,Win8.1SE,Run,%ScriptFile%,XP2k3NotCompatible
If,%ProjectName%,Equal,Win10PESE,Run,%ScriptFile%,XP2k3NotCompatible
End

[XP2k3NotCompatible]
Message,"%ProjectName% uses ADK for %OS%, it is unfortunately not compatible with Windows XP, 2003 and Vista HostOS.#$x#$xExiting...",Error
Halt,"%ProjectName% uses ADK for %OS%, it is unfortunately not compatible with Windows XP, 2003 and Vista HostOS. Exiting..."

[NoMediaCreationToolISO]
If,ExistFile,%SourceDir%\MediaMeta.xml,Call,Echo,"Do NOT use the media creation tool MediaCreationTool(x64).exe#$xto download Windows.#$x#$xYou can use the button 'Download Official Windows 10 ISO' on Main Plugin.",Warn,,MessageError,,Halt

[Process]
Set,%PType%,2
If,%OS%,Equal,Win10,Begin
If,%BuildClass_ScrollBox%,Equal,"BuildClass 3",Set,%PType%,3
End
Set,%BuildClass%,%PType%,Permanent
//-
//Start with null values - We do not have a Reset Main Plugin like Azin for now - Changing new feature BuildClass 2 have these values 3 do not - channging 2 with 3 can cause old values to be used
Set,%BootSRC%,|:|,Permanent
Set,%InstallSRC%,|:|,Permanent
Set,%Source_Win%,|:|,Permanent
Set,%Source_Sys%,|:|,Permanent
IniWrite,%ProjectDir%\script.project,IniVariables,BootSRC,|:|
IniWrite,%ProjectDir%\script.project,IniVariables,InstallSRC,|:|
//-
If,%PType%,Equal,2,Begin
Set,%MountBootSrc%,%BaseDir%\Mount\%ProjectName%\Source\BootWimSrc
Set,%MountInstallSrc%,%BaseDir%\Mount\%ProjectName%\Source\InstallWimSrc
End
//-
Run,%ScriptFile%,CheckHostOS
Run,%ScriptFile%,PrepPE
Run,%ScriptFile%,NoMediaCreationToolISO
//run HostOSInfoMini if not done in main configuration ( Run,%ScriptFile%,HostOSInfoMini )
Call,HostOSArch
//-
//Before Auto(Quoestion=-Extract Wim Folders -- Early Check if source supported (after 1809 Win10PESE not updated for new versions)
Set,%t%,-
If,%OS%,Equal,Win10,Call,IniReadCSVQ,%ProjectDir%\Build\0-preconfig.script,Interface,NOCheck_CheckBox,8,t
If,%OS%_%t%,Equal,Win10_False,Exec,%ProjectDir%\Build\0-preconfig.script,CheckWin10RTM
//-
If,%PType%,Equal,2,Run,%ScriptFile%,SetWimTools
//For now Add WIMToolsMode for BuildClass 3 to avoid error at PreConfig ...
If,%BuildClass%,Equal,3,IniWrite,%ProjectDir%\script.project,IniVariables,WIMToolsMode,%pScrollB_WimToolsMode%
//-
If,%ProjectName%,Equal,Win7PESE,Run,%ScriptFile%,CurrentWimTools_Win7PESE
//-
If,ExistDir,%ProjectTemp%\WimInfo,ShellExecute,Hide,cmd.exe,"/C rd /s /q #$q%ProjectTemp%\WimInfo#$q"
If,ExistDir,%ProjectTemp%\WimInfo,Call,DirDelete,%ProjectTemp%\WimInfo
DirMake,%ProjectTemp%\WimInfo
If,%PType%,Equal,2,Begin
//Better have WimInfoInstall with Extracted wim Folders to get wimindex number which is required by wimlib-imagex.exe to extract from correct wimimage no
//If,%ExtractWimFolder%,Equal,Yes,Run,%ScriptFile%,WimInfoExtract
//If,%ExtractWimFolder%,Equal,Yes,Run,%ScriptFile%,WimInfoInstall
If,%ExtractWimFolder%,Equal,Yes,Begin
Set,%t%,1
If,Not,ExistFile,%SourceDir%\sources\install.wim,Set,%t%,0
If,Not,ExistFile,%SourceDir%\sources\install.esd,Set,%t%,0
//Hybrid - "Extracted wim Folders" and "install.wim" (install.esd) together as designed. - Faster Build - Gain Speed advantage of both "Extracted wim Folders" and extracting from "install.wim" (install.esd)
If,%t%,Equal,1,Run,%ScriptFile%,WimInfoInstall
//Special - "Extracted wim Folders" with manually deleted "install.wim" (install.esd) - Slower Build - Gain Speed advantage of only "Extracted wim Folders" eg. XenonKilla
If,%t%,Equal,0,Run,%ScriptFile%,WimInfoExtract
//-
//Get Index Number from Extracted Install Wim Folder name...
Set,%t%,%pInstallWimSrcBox%
StrFormat,Right,%t%,1,%t2%
If,%t2%,Equal,\,StrFormat,RTrim,%t%,1,%t%
StrFormat,FileName,%t%,%t%
IniWrite,%ProjectDir%\script.project,IniVariables,WIMIndex,%t%
Set,%WIMIndex%,%t%
//-
End
If,%ExtractWimFolder%,Equal,No,Run,%ScriptFile%,WimInfoInstall
End
If,%PType%,Equal,3,Run,%ScriptFile%,WimInfoInstall
Run,%ScriptFile%,CheckOSproject
If,%OS%,Equal,Win7,Run,%ScriptFile%,CheckWin7RTM
If,%OS%,Equal,Win8,Run,%ScriptFile%,CheckWin8RTM
If,%OS%,Equal,Win8.1,Run,%ScriptFile%,CheckWin8.1RTM
If,%OS%,Equal,Win10,Run,%ScriptFile%,CheckWin10RTM
Run,%ScriptFile%,Process_TargetFinalDir
Run,%ScriptFile%,CommonSettings
Run,%ScriptFile%,WIMInterfaceInfo
IniWrite,%ProjectDir%\script.project,IniVariables,OS,%OS%
IniWrite,%ProjectDir%\script.project,IniVariables,OSSubType,%WIMEditionID%
IniWrite,%ProjectDir%\script.project,IniVariables,RegistryCache,%ProjectCache%\%WIMName%_%WIMEditionID%_%OSVersion%_%SourceArch%_%DistLang%
//-
If,%ExtractWimFolder%-%InstallWimCompression%,Equal,No-LZMS,Run,%ScriptFile%,UseExtractWimforESD
//--
Echo,OS=%OS%
Echo,OSServicePack=%OSServicePack%
Echo,OSVersion=%OSVersion%
Echo,WIMName=%WIMName%
Echo,WIMEditionID=%WIMEditionID%
Echo,SourceArch=%SourceArch%
Echo,DistLang=%DistLang%
Echo,WIMIndex=%WIMIndex%
Echo,WIMImageCount=%WIMImageCount%
Echo,WIMToolsMode=%WIMToolsMode%
Echo,WIMDriver=%WIMDriver%
Echo,ShowImageX=%ShowImageX%
Echo,ShowWimUtil=%ShowWimUtil%
Echo,ExtractWimFolder=%ExtractWimFolder%

[Extract_WimFolders_Auto]
Run,%ScriptFile%,Extract_WimFolders_Check

[Extract_WimFolders_Check]
//First Check Extracted
Set,%t%,0
//Following similar lines at [SetWimTools] [Extract_WimFolders_Check]
