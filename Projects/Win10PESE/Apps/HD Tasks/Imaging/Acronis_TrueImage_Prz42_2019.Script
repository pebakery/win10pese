If,ExistDir,%Target_Prog%\%ProgramFolder%\IScsi,Call,DirDelete,%Target_Prog%\%ProgramFolder%\IScsi
//-
Set,%targt%,%Target_Sys%
Set,%wow64Node%,""
Set,%targt_Reg%,x:\Windows\System32
If,%Pure64bit%,Equal,False,Begin
If,%SourceArch%,Equal,x64,Begin
Set,%targt%,%Target_Win%\SysWOW64
Set,%wow64Node%,"Wow6432Node\"
Set,%targt_Reg%,x:\Windows\SysWOW64
End
End
//-
If,%targt%,Equal,%Target_Win%\SysWOW64,Begin
If,ExistFile,%Target_Sys%\snapapi.dll,Echo,"Another Acronis Product Installed 64 bit Product and this program will not work",Warn
End
Call,FileCopyBiggerVersion,%ProjectTemp%\TempExtractFolder\%ProgramFolderS%\Files\Drivers\snapapint.dll,%targt%
Call,FileCopyBiggerVersion,%ProjectTemp%\TempExtractFolder\%ProgramFolderS%\Files\Drivers\snapapint64.dll,%targt%
Call,FileCopyBiggerVersion,%ProjectTemp%\TempExtractFolder\%ProgramFolderS%\%t%\Drivers\snapapi.dll,%targt%
If,%SourceArch%,Equal,x64,Set,%sd%,%ProjectTemp%\TempExtractFolder\%ProgramFolderS%\Files64
Else,Set,%sd%,%ProjectTemp%\TempExtractFolder\%ProgramFolderS%\Files32
Call,FileCopyBiggerVersion,%sd%\Drivers\fltsrv.sys,%Target_Sys%\Drivers
Call,FileCopyBiggerVersion,%sd%\Drivers\snapman.sys,%Target_Sys%\Drivers
