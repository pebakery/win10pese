End
If,Not,ExistFile,%Tools%\x64\Dism\dism.exe,If,ExistFile,%GWTAdkFolder%\amd64\dism\dism.exe,Begin
If,Not,ExistDir,%Tools%\x64\Dism,DirMake,%Tools%\x64\Dism
FileCopy,%GWTAdkFolder%\amd64\dism\*,%Tools%\x64\Dism
End

[CopyDevcon]
If,Not,ExistFile,%Tools%\x86\devcon.exe,If,ExistFile,%GWTDevconFolder%\x86\devcon.exe,FileCopy,%GWTDevconFolder%\x86\devcon.exe,%Tools%\x86\devcon.exe
If,Not,ExistFile,%Tools%\x64\devcon.exe,If,ExistFile,%GWTDevconFolder%\amd64\devcon.exe,FileCopy,%GWTDevconFolder%\amd64\devcon.exe,%Tools%\x64\devcon.exe

[CheckInfFiles]
Echo,"Prepare setup information drivers files..."
If,%GWTWaik2%,Equal,True,If,Not,ExistFile,%Tools%\x86\wimfltr.inf,Begin
Set,%w%,%Tools%\x86\wimfltr.inf
Run,%scriptfile%,WimFltrInfFile
End
If,%GWTWaik2%,Equal,True,If,Not,ExistFile,%Tools%\x64\wimfltr.inf,Begin
Set,%w%,%Tools%\x64\wimfltr.inf
Run,%scriptfile%,WimFltrInfFile
End
If,Not,ExistFile,%Tools%\x86\wimmount.inf,Begin
Set,%w%,%Tools%\x86\wimmount.inf
Run,%scriptfile%,WimMountInfFile
End
If,Not,ExistFile,%Tools%\x64\wimmount.inf,Begin
Set,%w%,%Tools%\x64\wimmount.inf
Run,%scriptfile%,WimMountInfFile
End

[HostOSInfoMini]
Echo,"Detect HostOS Info..."
If,Not,ExistDir,%ProjectTemp%,DirMake,%ProjectTemp%
If,ExistFile,%ProjectTemp%\InfoHostOSMiniIni.ini,Shellexecute,Hide,cmd.exe,"/C del /q /f #$q%ProjectTemp%\InfoHostOSMiniIni.ini#$q"
If,ExistFile,%ProjectTemp%\InfoHostOSMiniIni.ini,Echo,"FILE Not deleted",Warn
If,ExistFile,%ProjectTemp%\InfoHostOSMiniIni.au3,Shellexecute,Hide,cmd.exe,"/C del /q /f #$q%ProjectTemp%\InfoHostOSMiniIni.au3#$q"
If,ExistFile,%ProjectTemp%\InfoHostOSMiniIni.au3,Echo,"FILE Not deleted",Warn
Set,%w%,%ProjectTemp%\InfoHostOSMini.au3
FileCreateBlank,%w%
TxtAddline,%w%,"#NoTrayIcon",Append
TxtAddline,%w%,"#RequireAdmin",Append
TxtAddline,%w%,"IniWrite(#$qInfoHostOSMiniIni.ini#$q,#$qinfohostos#$q,#$qosarch#$q,@OSArch)",Append
//Following MUST me x86 NOT %systype% or %OsArch% ..... ;).
ShellExecute,Hide,%Tools%\x86\AU3381.exe,"#$q%w%#$q",%ProjectTemp%
IniRead,%ProjectTemp%\InfoHostOSMiniIni.ini,infohostos,OSArch,%SysTypeVar%
Set,%SysType%,%SysTypeVar%,Permanent
Set,%OSArch%,%SysTypeVar%,Permanent
FileDelete,%ProjectTemp%\InfoHostOSMiniIni.au3
