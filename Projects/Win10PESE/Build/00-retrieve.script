[Process]
Echo,"%ScriptTitle% For %ProjectName%"
IniRead,%projectdir%\script.project,IniVariables,WIMImageCount,%WIMImageCount%
//run HostOSInfoMini if not done in main configuration
If,%OSArch%,Equal,"",Run,%scriptfile%,HostOSInfoMini
If,%SysType%,Equal,"",Run,%scriptfile%,HostOSInfoMini
//-- preparing HostOS tools
Run,%scriptfile%,HostOSTools
If,ExistFile,%ProjectTemp%\HostOSTools\cmd.exe,Set,%CmdexeFolder%,%ProjectTemp%\HostOSTools\
//-- Get - Retrieve wim tools
If,%ScrollBox_WaikTools%,Equal,GetWaikTools,Run,%scriptfile%,GetWaikTools
Else,Exit,"RetrieveWimTools (From install Waik/ADK, Win10/8/7 HostOS, install.wim) not written for %ProjectName% !!!"
//-- check Inf files
Run,%scriptfile%,CheckInfFiles

[Launch_DownloadWTTools]
If,Not,ExistFile,%ProjectDir%\Utils\GetWaikTools.script,Message,"GetWaikTools Plugin required to dowload Waik/ADK Tools !!!",Error
Else,Begin
// fix error on CopyAdk section for x86\wofadk.sys (only Win10PESE)
If,%ProjectName%,Equal,Win10PESE,Begin
If,ExistFile,%Tools%\x86\wofadk.sys,Retrieve,FILESIZE,%Tools%\x86\wofadk.sys,%wofadkSize%
If,%wofadkSize%,Smaller,190000,Begin
FileDelete,%Tools%\x86\wofadk.sys
If,ExistFile,%Tools%\x86\wofadk.sys,Call,FileDelete,%Tools%\x86\wofadk.sys
End
End
If,%GWTWaik2%,Equal,True,Set,%Waik2Tools%,%GWTWaik2Switch%
Else,Set,%Waik2Tools%,
If,%GWTAdk%,Equal,True,Set,%AdkTools%,%GWTAdkSwitch%
Else,Set,%AdkTools%,
If,%GWTDism%,Equal,True,Set,%WaikDism%,%GWTDismSwitch%
Else,Set,%WaikDism%,
If,Not,%GWTwofadkSwitch%,Equal,%GWTAdkSwitch%,Set,%ADKDism10%,%GWTwofadkSwitch%
Else,Set,%ADKDism10%,
If,%GWTDevcon%,Equal,True,Set,%WDKdevcon%,%GWTDevconSwitch%
Else,Set,%WDKdevcon%,
Echo,"Download %Waik2Tools% %AdkTools% %WaikDism% %WDKdevcon% Waik/ADK tools ..."
Run,%ProjectDir%\Utils\GetWaikTools.script,Launch_Program,%ProjectDir%\Utils\GetWaikTools.script,"%Waik2Tools% %AdkTools% %WaikDism% %ADKDism10% %WDKdevcon% -Silent"
// Recover from %GlobalTemplates%\GetWaikTools if exist
If,%GWTWaik2%,Equal,True,If,Not,-%Waik2Tools%,Equal,-,Run,%scriptfile%,CopyWaik2
If,%GWTAdk%,Equal,True,If,Not,-%AdkTools%,Equal,-,Run,%scriptfile%,CopyAdk
If,Not,%GWTwofadkSwitch%,Equal,%GWTAdkSwitch%,Run,%scriptfile%,CopyWofadk
If,%GWTDism%,Equal,True,If,Not,-%WaikDism%,Equal,-,Run,%scriptfile%,CopyDism
If,%GWTDevcon%,Equal,True,If,Not,-%WDKdevcon%,Equal,-,Run,%scriptfile%,CopyDevcon
End

[GetWaikTools]
// fix error on CopyAdk section for x86\wofadk.sys (only Win10PESE)
If,%ProjectName%,Equal,Win10PESE,Begin
If,ExistFile,%Tools%\x86\wofadk.sys,Retrieve,FILESIZE,%Tools%\x86\wofadk.sys,%wofadkSize%
If,%wofadkSize%,Smaller,190000,Begin
FileDelete,%Tools%\x86\wofadk.sys
If,ExistFile,%Tools%\x86\wofadk.sys,Call,FileDelete,%Tools%\x86\wofadk.sys
End
End
Run,%scriptfile%,CheckWaikTools
// Recover from %GlobalTemplates%\GetWaikTools if exist first
If,%GWTWaik2%,Equal,True,If,Not,-%Waik2Tools%,Equal,-,Run,%scriptfile%,CopyWaik2
If,%GWTAdk%,Equal,True,If,Not,-%AdkTools%,Equal,-,Run,%scriptfile%,CopyAdk
If,Not,%GWTwofadkSwitch%,Equal,%GWTAdkSwitch%,Run,%scriptfile%,CopyWofadk
If,%GWTDism%,Equal,True,If,Not,-%WaikDism%,Equal,-,Run,%scriptfile%,CopyDism
If,%GWTDevcon%,Equal,True,If,Not,-%WDKdevcon%,Equal,-,Run,%scriptfile%,CopyDevcon
Run,%scriptfile%,CheckWaikTools
//- Download with GetWaikTools
If,Not,-%Waik2Tools%%AdkTools%%WaikDism%%ADKDism10%%WDKdevcon%,Equal,-,Begin
If,Not,ExistFile,%ProjectDir%\Utils\GetWaikTools.script,Begin
Message,"GetWaikTools Plugin required to download Waik/ADK Tools !!!#$x#$xExiting...",Error
Halt,"GetWaikTools Plugin required to download Waik/ADK Tools. Exiting..."
End
Echo,"Download %Waik2Tools% %AdkTools% %WaikDism% %ADKDism10% %WDKdevcon% Waik/ADK tools ..."
Run,%ProjectDir%\Utils\GetWaikTools.script,Launch_Program,%ProjectDir%\Utils\GetWaikTools.script,"%Waik2Tools% %AdkTools% %WaikDism% %ADKDism10% %WDKdevcon% -Silent"
// Recover from %GlobalTemplates%\GetWaikTools if exist
If,%GWTWaik2%,Equal,True,If,Not,-%Waik2Tools%,Equal,-,Run,%scriptfile%,CopyWaik2
If,%GWTAdk%,Equal,True,If,Not,-%AdkTools%,Equal,-,Run,%scriptfile%,CopyAdk
If,Not,%GWTwofadkSwitch%,Equal,%GWTAdkSwitch%,Run,%scriptfile%,CopyWofadk
If,%GWTDism%,Equal,True,If,Not,-%WaikDism%,Equal,-,Run,%scriptfile%,CopyDism
If,%GWTDevcon%,Equal,True,If,Not,-%WDKdevcon%,Equal,-,Run,%scriptfile%,CopyDevcon
End

[CheckWaikTools]
// Waik_2
Set,%Waik2Tools%,
If,%GWTWaik2%,Equal,True,Begin
If,Not,ExistFile,%Tools%\peimg.exe,Set,%Waik2Tools%,%GWTWaik2Switch%
If,Not,ExistFile,%Tools%\ssshim.dll,Set,%Waik2Tools%,%GWTWaik2Switch%
// Waik_2 x86
If,Not,ExistFile,%Tools%\x86\imagex6.0.exe,Set,%Waik2Tools%,%GWTWaik2Switch%
If,Not,ExistFile,%Tools%\x86\wimfltr.inf,Set,%Waik2Tools%,%GWTWaik2Switch%
If,Not,ExistFile,%Tools%\x86\wimfltr.sys,Set,%Waik2Tools%,%GWTWaik2Switch%
If,Not,ExistFile,%Tools%\x86\wimgapi6.0.dll,Set,%Waik2Tools%,%GWTWaik2Switch%
// Waik_2 x64
If,Not,ExistFile,%Tools%\x64\imagex6.0.exe,Set,%Waik2Tools%,%GWTWaik2Switch%
If,Not,ExistFile,%Tools%\x64\wimfltr.inf,Set,%Waik2Tools%,%GWTWaik2Switch%
If,Not,ExistFile,%Tools%\x64\wimfltr.sys,Set,%Waik2Tools%,%GWTWaik2Switch%
If,Not,ExistFile,%Tools%\x64\wimgapi6.0.dll,Set,%Waik2Tools%,%GWTWaik2Switch%
End
// ADK_4(_1) x86
Set,%AdkTools%,
Set,%ADKDism10%,
If,%GWTAdk%,Equal,True,Begin
If,Not,ExistFile,%Tools%\x86\bcdboot.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\bcdedit.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\bootsect.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\imagex.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\oscdimg.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\wimgapi.dll,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\wimmount.sys,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\WimMountInstall.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\wimserv.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x86\wofadk.sys,Begin
If,%GWTwofadkSwitch%,Equal,%GWTAdkSwitch%,Set,%AdkTools%,%GWTAdkSwitch%
Else,Set,%ADKDism10%,%GWTwofadkSwitch%
End
// ADK_4(_1) x64
If,Not,ExistFile,%Tools%\x64\bcdboot.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\bcdedit.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\bootsect.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\imagex.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\oscdimg.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\wimgapi.dll,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\wimmount.sys,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\WimMountInstall.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\wimserv.exe,Set,%AdkTools%,%GWTAdkSwitch%
If,Not,ExistFile,%Tools%\x64\wofadk.sys,Begin
If,%GWTwofadkSwitch%,Equal,%GWTAdkSwitch%,Set,%AdkTools%,%GWTAdkSwitch%
Else,Set,%ADKDism10%,%GWTwofadkSwitch%
End
End
// Dism x86/x64
Set,%WaikDism%,
If,%GWTDism%,Equal,True,Begin
If,Not,ExistFile,%Tools%\x86\Dism\dism.exe,Set,%WaikDism%,%GWTDismSwitch%
If,Not,ExistFile,%Tools%\x64\Dism\dism.exe,Set,%WaikDism%,%GWTDismSwitch%
End
// Devcon x86/x64
Set,%WDKdevcon%,
If,%GWTDevcon%,Equal,True,Begin
If,Not,ExistFile,%Tools%\x86\devcon.exe,Set,%WDKdevcon%,%GWTDevconSwitch%
If,Not,ExistFile,%Tools%\x64\devcon.exe,Set,%WDKdevcon%,%GWTDevconSwitch%
End

[CopyWaik2]
// Waik_2
If,Not,ExistFile,%Tools%\peimg.exe,If,ExistFile,%GWTWaik2Folder%\x86\peimg.exe,FileCopy,%GWTWaik2Folder%\x86\peimg.exe,%Tools%\peimg.exe
If,Not,ExistFile,%Tools%\ssshim.dll,If,ExistFile,%GWTWaik2Folder%\x86\ssshim.dll,FileCopy,%GWTWaik2Folder%\x86\ssshim.dll,%Tools%\ssshim.dll
// Waik_2 x86
If,Not,ExistFile,%Tools%\x86\imagex6.0.exe,If,ExistFile,%GWTWaik2Folder%\x86\imagex.exe,FileCopy,%GWTWaik2Folder%\x86\imagex.exe,%Tools%\x86\imagex6.0.exe
If,Not,ExistFile,%Tools%\x86\wimfltr.inf,If,ExistFile,%GWTWaik2Folder%\x86\wimfltr.inf,FileCopy,%GWTWaik2Folder%\x86\wimfltr.inf,%Tools%\x86\wimfltr.inf
If,Not,ExistFile,%Tools%\x86\wimfltr.sys,If,ExistFile,%GWTWaik2Folder%\x86\wimfltr.sys,FileCopy,%GWTWaik2Folder%\x86\wimfltr.sys,%Tools%\x86\wimfltr.sys
If,Not,ExistFile,%Tools%\x86\wimgapi6.0.dll,If,ExistFile,%GWTWaik2Folder%\x86\wimgapi.dll,FileCopy,%GWTWaik2Folder%\x86\wimgapi.dll,%Tools%\x86\wimgapi6.0.dll
// Waik_2 x64
If,Not,ExistFile,%Tools%\x64\imagex6.0.exe,If,ExistFile,%GWTWaik2Folder%\amd64\imagex.exe,FileCopy,%GWTWaik2Folder%\amd64\imagex.exe,%Tools%\x64\imagex6.0.exe
If,Not,ExistFile,%Tools%\x64\wimfltr.inf,If,ExistFile,%GWTWaik2Folder%\amd64\wimfltr.inf,FileCopy,%GWTWaik2Folder%\amd64\wimfltr.inf,%Tools%\x64\wimfltr.inf
If,Not,ExistFile,%Tools%\x64\wimfltr.sys,If,ExistFile,%GWTWaik2Folder%\amd64\wimfltr.sys,FileCopy,%GWTWaik2Folder%\amd64\wimfltr.sys,%Tools%\x64\wimfltr.sys
If,Not,ExistFile,%Tools%\x64\wimgapi6.0.dll,If,ExistFile,%GWTWaik2Folder%\amd64\wimgapi.dll,FileCopy,%GWTWaik2Folder%\amd64\wimgapi.dll,%Tools%\x64\wimgapi6.0.dll

[CopyAdk]
// ADK_4(_1) x86
If,Not,ExistFile,%Tools%\x86\bcdboot.exe,If,ExistFile,%GWTAdkFolder%\x86\BCDBoot\bcdboot.exe,FileCopy,%GWTAdkFolder%\x86\BCDBoot\bcdboot.exe,%Tools%\x86\bcdboot.exe
If,Not,ExistFile,%Tools%\x86\bcdedit.exe,If,ExistFile,%GWTAdkFolder%\x86\BCDBoot\bcdedit.exe,FileCopy,%GWTAdkFolder%\x86\BCDBoot\bcdedit.exe,%Tools%\x86\bcdedit.exe
If,Not,ExistFile,%Tools%\x86\bootsect.exe,If,ExistFile,%GWTAdkFolder%\x86\BCDBoot\bootsect.exe,FileCopy,%GWTAdkFolder%\x86\BCDBoot\bootsect.exe,%Tools%\x86\bootsect.exe
If,Not,ExistFile,%Tools%\x86\imagex.exe,If,ExistFile,%GWTAdkFolder%\x86\dism\imagex.exe,FileCopy,%GWTAdkFolder%\x86\dism\imagex.exe,%Tools%\x86\imagex.exe
If,Not,ExistFile,%Tools%\x86\wimgapi.dll,If,ExistFile,%GWTAdkFolder%\x86\dism\wimgapi.dll,FileCopy,%GWTAdkFolder%\x86\dism\wimgapi.dll,%Tools%\x86\wimgapi.dll
If,Not,ExistFile,%Tools%\x86\wimmount.sys,If,ExistFile,%GWTAdkFolder%\x86\dism\wimmount.sys,FileCopy,%GWTAdkFolder%\x86\dism\wimmount.sys,%Tools%\x86\wimmount.sys
If,Not,ExistFile,%Tools%\x86\WimMountInstall.exe,If,ExistFile,%GWTAdkFolder%\x86\dism\WimMountInstall.exe,FileCopy,%GWTAdkFolder%\x86\dism\WimMountInstall.exe,%Tools%\x86\WimMountInstall.exe
If,Not,ExistFile,%Tools%\x86\wimserv.exe,If,ExistFile,%GWTAdkFolder%\x86\dism\wimserv.exe,FileCopy,%GWTAdkFolder%\x86\dism\wimserv.exe,%Tools%\x86\wimserv.exe
If,Not,ExistFile,%Tools%\x86\wofadk.sys,If,ExistFile,%GWTAdkFolder%\x86\dism\wofadk.sys,FileCopy,%GWTAdkFolder%\x86\dism\wofadk.sys,%Tools%\x86\wofadk.sys
If,Not,ExistFile,%Tools%\x86\oscdimg.exe,If,ExistFile,%GWTAdkFolder%\x86\Oscdimg\oscdimg.exe,FileCopy,%GWTAdkFolder%\x86\Oscdimg\oscdimg.exe,%Tools%\x86\oscdimg.exe
// ADK_4(_1) x64
If,Not,ExistFile,%Tools%\x64\bcdboot.exe,If,ExistFile,%GWTAdkFolder%\amd64\BCDBoot\bcdboot.exe,FileCopy,%GWTAdkFolder%\amd64\BCDBoot\bcdboot.exe,%Tools%\x64\bcdboot.exe
If,Not,ExistFile,%Tools%\x64\bcdedit.exe,If,ExistFile,%GWTAdkFolder%\amd64\BCDBoot\bcdedit.exe,FileCopy,%GWTAdkFolder%\amd64\BCDBoot\bcdedit.exe,%Tools%\x64\bcdedit.exe
If,Not,ExistFile,%Tools%\x64\bootsect.exe,If,ExistFile,%GWTAdkFolder%\amd64\BCDBoot\bootsect.exe,FileCopy,%GWTAdkFolder%\amd64\BCDBoot\bootsect.exe,%Tools%\x64\bootsect.exe
If,Not,ExistFile,%Tools%\x64\imagex.exe,If,ExistFile,%GWTAdkFolder%\amd64\dism\imagex.exe,FileCopy,%GWTAdkFolder%\amd64\dism\imagex.exe,%Tools%\x64\imagex.exe
If,Not,ExistFile,%Tools%\x64\wimgapi.dll,If,ExistFile,%GWTAdkFolder%\amd64\dism\wimgapi.dll,FileCopy,%GWTAdkFolder%\amd64\dism\wimgapi.dll,%Tools%\x64\wimgapi.dll
If,Not,ExistFile,%Tools%\x64\wimmount.sys,If,ExistFile,%GWTAdkFolder%\amd64\dism\wimmount.sys,FileCopy,%GWTAdkFolder%\amd64\dism\wimmount.sys,%Tools%\x64\wimmount.sys
If,Not,ExistFile,%Tools%\x64\WimMountInstall.exe,If,ExistFile,%GWTAdkFolder%\amd64\dism\WimMountInstall.exe,FileCopy,%GWTAdkFolder%\amd64\dism\WimMountInstall.exe,%Tools%\x64\WimMountInstall.exe
If,Not,ExistFile,%Tools%\x64\wimserv.exe,If,ExistFile,%GWTAdkFolder%\amd64\dism\wimserv.exe,FileCopy,%GWTAdkFolder%\amd64\dism\wimserv.exe,%Tools%\x64\wimserv.exe
If,Not,ExistFile,%Tools%\x64\wofadk.sys,If,ExistFile,%GWTAdkFolder%\amd64\dism\wofadk.sys,FileCopy,%GWTAdkFolder%\amd64\dism\wofadk.sys,%Tools%\x64\wofadk.sys
