[Called_Process]
Set,%Locale%,#1
//--
IniRead,%ProjectDir%\script.project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
//-
Echo,"Processing Locale %Locale%..."
If,%Locale%,Equal,Auto,Run,%LocaleScript%,Auto
Else,If,Not,-%Locale%,Equal,-,Run,%LocaleScript%,Run_Locale_Choice

[Process]
// Comment Exit for tests
Exit,"The plugin is designed to be called from other plugin"
Set,%Locale%,%pScrollB_Locale%
//--
IniRead,%ProjectDir%\script.project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
//-
Echo,"Processing Locale %Locale%..."
If,%Locale%,Equal,Auto,Run,%LocaleScript%,Auto
Else,If,Not,-%Locale%,Equal,-,Run,%LocaleScript%,Run_Locale_Choice

[Run_Locale_Choice]
If,%Locale%,Equal,"Belgian French",Run,%LocaleScript%,fr-BE
If,%Locale%,Equal,French,Run,%LocaleScript%,fr-FR
If,%Locale%,Equal,Hebrew,Run,%LocaleScript%,he-IL
If,%Locale%,Equal,German,Run,%LocaleScript%,de-DE
If,%Locale%,Equal,Greek,Run,%LocaleScript%,el-GR
If,%Locale%,Equal,Hungarian,Run,%LocaleScript%,hu-HU
If,%Locale%,Equal,Italian,Run,%LocaleScript%,it-IT
If,%Locale%,Equal,Korean,Run,%LocaleScript%,ko-KR
If,%Locale%,Equal,Portuguese,Run,%LocaleScript%,pt-PT
If,%Locale%,Equal,Russian,Run,%LocaleScript%,ru-RU
If,%Locale%,Equal,Spanish,Run,%LocaleScript%,es-ES
If,%Locale%,Equal,Swedish,Run,%LocaleScript%,sv-SE
If,%Locale%,Equal,Turkish,Run,%LocaleScript%,tr-TR
If,%Locale%,Equal,"UK English",Run,%LocaleScript%,en-GB
// Auto: If exists in local archive (**temporary solution)
If,%Locale%,Equal,Auto,Begin
If,ExistSection,%LocaleScript%,%DistLang%,Run,%LocaleScript%,%DistLang%
End
// Add NLS files and fonts, if does not already exist, according to the Registry for specified language
Run,%LocaleScript%,Auto

[minimum_NLS_tr-TR]
//Minimum source tr-TR support to have bootable cd (Thanks to BlueLife). minimum C_1254.NLS and C_857.NLS are needed (else black screen at boot)
If,%BuildClass%,Equal,2,Begin
If,Not,ExistFile,%TargetDir%\Windows\System32\C_1254.NLS,FileCopy,%BootSRC%\Windows\System32\C_1254.NLS,%TargetDir%\Windows\System32
If,Not,ExistFile,%TargetDir%\Windows\System32\C_857.NLS,FileCopy,%BootSRC%\Windows\System32\C_857.NLS,%TargetDir%\Windows\System32
End
If,%BuildClass%,Equal,3,Begin
If,Not,ExistFile,%TargetDir%\Windows\System32\C_1254.NLS,TxtAddLine,%PInstall%_After_B_I_B.txt,"\Windows\System32\C_1254.NLS",Append
If,Not,ExistFile,%TargetDir%\Windows\System32\C_857.NLS,TxtAddLine,%PInstall%_After_B_I_B.txt,"\Windows\System32\C_857.NLS",Append
End

