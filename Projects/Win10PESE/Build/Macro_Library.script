ShellExecuteex,Open,cmd.exe,"/k Echo Test"
ShellExecuteex,Open,%ProjectTemp%\Test_cmd\cmd.exe,"/k Echo Temp_cmd Test"
ShellExecuteex,Open,%ProjectTemp%\Test_cmd\cmd.exe,"/k Echo Temp_cmd Test"
ShellExecuteex,Open,%ProjectTemp%\Test_cmd\cmd.exe,"/k Echo Temp_cmd Test"
System,FileRedirect,ON
Call,WaitProcess,ParentProcess,cmd.exe,5
