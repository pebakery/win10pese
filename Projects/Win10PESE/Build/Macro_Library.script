StrFormat,Replace,#4,True,-,#4
StrFormat,Replace,#4,False,-,#4
If,#4,Equal,-,Begin
StrFormat,Dec,#5,1
StrFormat,Split,#9,|,#5,#4
StrFormat,Left,#4,1,#5
StrFormat,Right,#4,1,#4
If,#4#5,Equal,__,Begin
Set,#5,#8
StrFormat,Dec,#5,1
StrFormat,Split,#9,|,#5,#4
Set,#6,#4
StrFormat,Dec,#5,1
StrFormat,Split,#9,|,#5,#4
Set,#6,#4|#6
StrFormat,Dec,#5,1
StrFormat,Split,#9,|,#5,#4
Set,#6,#4|#6
Run,%api%,CallC_ResetInterface_ScrollBox_Write,#1,#2,#3,#4,#5,#6,#7,#8,#9
End
End
