[Called_process]
Set,%FormatScript%,#1
If,-#1,Equal,-,Set,%FormatScript%,%ScriptFile%
Set,%Format%,#2
//--
IniRead,%ProjectDir%\script.project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
//-
Echo,"Processing Format %Format%..."
If,%Format%,Equal,Auto,Run,%FormatScript%,Auto
Else,If,Not,-%Format%,Equal,-,Run,%FormatScript%,Run_Format_Choice

[Process]
// Comment Exit for tests
Exit,"The Plugin is designed to be called from other Plugin"
Set,%FormatScript%,%ScriptFile%
Set,%Format%,%pScroll_Format%
//--
IniRead,%ProjectDir%\script.project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
//-
Echo,"Processing Format %Format%..."
If,%Format%,Equal,Auto,Run,%FormatScript%,Auto
Else,If,Not,-%Format%,Equal,-,Run,%FormatScript%,Run_Format_Choice

[Run_Format_Choice]
RegHiveLoad,Tmp_Default,%RegDefault%
If,%Format%,Equal,"Belgian French",Run,%FormatScript%,fr-BE
If,%Format%,Equal,French,Run,%FormatScript%,fr-FR
If,%Format%,Equal,Hebrew,Run,%FormatScript%,he-IL
If,%Format%,Equal,German,Run,%FormatScript%,de-DE
If,%Format%,Equal,Greek,Run,%FormatScript%,el-GR
If,%Format%,Equal,Hungarian,Run,%FormatScript%,hu-HU
If,%Format%,Equal,Italian,Run,%FormatScript%,it-IT
If,%Format%,Equal,Korean,Run,%FormatScript%,ko-KR
If,%Format%,Equal,Portuguese,Run,%FormatScript%,pt-PT
If,%Format%,Equal,Russian,Run,%FormatScript%,ru-RU
If,%Format%,Equal,Spanish,Run,%FormatScript%,es-ES
If,%Format%,Equal,Swedish,Run,%FormatScript%,sv-SE
If,%Format%,Equal,Turkish,Run,%FormatScript%,tr-TR
If,%Format%,Equal,"UK English",Run,%FormatScript%,en-GB
// Auto: If exists in local archive (**temporary solution)
If,%Format%,Equal,Auto,Begin
If,ExistSection,%FormatScript%,%DistLang%,Run,%FormatScript%,%DistLang%
Else,Run,%FormatScript%,Auto
End
RegHiveUnLoad,Tmp_Default

[Auto]
Echo,"Processing Auto Format..."
