If,%BuildType%-,Equal,-,Set,%BuildType%,PE
//--
RegHiveLoad,Tmp_Software,%RegSoftware%
//Remove Libraries Icons... Begin
//Rename folder in explorer to catch the good description and icons
//Folder not found issue
//All these library values are BWIM & IWIM hive default values
If,%RemoveLibIcons_Warn_CheckBox%,Equal,True,Run,%ScriptFile%,Process_RemoveLibIcons_Warn
If,%RemoveLibIcons_CheckBox%,Equal,True,Run,%ScriptFile%,Process_RemoveLibIcons
//Remove Libraries Icons... End
RegHiveUnLoad,Tmp_software
//  Write ComputerName
If,%CN_CheckBox%,Equal,True,Run,%ScriptFile%,WriteComputerName
//  Change Usb start
If,Not,%ProjectName%%BuildType%,Equal,Win8.1SEOS,Run,%ScriptFile%,UsbStart
//
// startup windows firewall to use network share with recovery environment
// Start Firewall is also managed in plugin "2-Config" and "Penetwork"
Run,%ScriptFile%,StartFirewallSrv
//
// Hide StartupFolder
If,%RemoveStartup_CheckBox%,Equal,True,Begin
If,ExistDir,"%TargetProfilesUserDir%\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\Startup",DirDelete,"%TargetProfilesUserDir%\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\Startup"
If,ExistDir,"%TargetDir%\ProgramData\Microsoft\Windows\Start Menu\Programs\Startup",DirDelete,"%TargetDir%\ProgramData\Microsoft\Windows\Start Menu\Programs\Startup"
End
If,%RemoveAccessories_CheckBox%,Equal,True,Begin
//This will Break MultiLangauge Support - only useful if one have no Accessories shortcut (ex: Notepad)
If,ExistDir,"%TargetProfilesUserDir%\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\Accessories",DirDelete,"%TargetProfilesUserDir%\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\Accessories"
If,ExistDir,"%TargetDir%\ProgramData\Microsoft\Windows\Start Menu\Programs\Accessories",DirDelete,"%TargetDir%\ProgramData\Microsoft\Windows\Start Menu\Programs\Accessories"
End
If,%RemoveSystemTools_CheckBox%,Equal,True,Begin
//This will Break MultiLangauge Support - only useful if one have no System Tools shortcut (ex: CMD)
If,ExistDir,"%TargetProfilesUserDir%\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\System Tools",DirDelete,"%TargetProfilesUserDir%\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\System Tools"
If,ExistDir,"%TargetDir%\ProgramData\Microsoft\Windows\Start Menu\Programs\System Tools",DirDelete,"%TargetDir%\ProgramData\Microsoft\Windows\Start Menu\Programs\System Tools"
End
// Hide Administrative Tool folder in Menu
If,%HideAdminTool_CheckBox%,Equal,True,Begin
If,ExistFile,%Target_Sys%\autorun.cmd,Begin
TXTDelLine,%target_sys%\autorun.cmd,@Echo off
TXTAddLine,%Target_Sys%\autorun.cmd,"If Exist #$q#$pSystemDrive#$p\ProgramData\Microsoft\Windows\Start Menu\Programs\Administrative Tools#$q Attrib +S +H #$q#$pSystemDrive#$p\ProgramData\Microsoft\Windows\Start Menu\Programs\Administrative Tools#$q",Prepend
TXTAddLine,%Target_Sys%\autorun.cmd,"@Echo off",Prepend
End
End
If,%NoMountLocalHD_CheckBox%,Equal,True,Run,%ScriptFile%,NoMountLocalHD
If,%DeleteTmp_CheckBox%,Equal,True,Run,%ScriptFile%,DeleteSecureProjectTemp
If,%FireWall_Shortcuts_CheckBox%,Equal,True,Run,%ScriptFile%,StartFirewallShortcuts
If,%DelDesktopdesktopini_CheckBox%,Equal,True,Begin
