Call,FileDelete,%IsoFolder%\%ISOfilenameWOextenstion%.gz
//-
If,ExistFile,%IsoFolder%\%ISOfilenameWOextenstion%.ISO.gz,Call,Echo,"Can NOT delete %IsoFolder%\%ISOfilenameWOextenstion%.ISO.gz #$x#$x GZip Compression Cancelled #$x#$xPress OK to continue.",Warn,,Message
If,ExistFile,%IsoFolder%\%ISOfilenameWOextenstion%.gz,Call,Echo,"Can NOT delete %IsoFolder%\%ISOfilenameWOextenstion%.gz #$x#$x GZip Compression Cancelled #$x#$xPress OK to continue.",Warn,,Message
If,ExistFile,%ISOFile%_GzipBackup,Call,Echo,"Can NOT delete %ISOFile%_GzipBackup #$x#$x GZip Compression Cancelled #$x#$xPress OK to continue.",Warn,,Message
//-
Set,%Gzip_Cancel%,0
If,ExistFile,%IsoFolder%\%ISOfilenameWOextenstion%.ISO.gz,Set,%Gzip_Cancel%,1
If,ExistFile,%IsoFolder%\%ISOfilenameWOextenstion%.gz,Set,%Gzip_Cancel%,1
If,ExistFile,%ISOFile%_GzipBackup,Set,%Gzip_Cancel%,1
//-
If,%Gzip_Cancel%,Equal,0,Begin
FileCopy,%ISOFile%,%ISOFile%_GzipBackup
Echo,"Gzip Compressing Please Wait...."
ShellExecute,Hide,%Tools%\gzip.exe,"-9 #$q%ISOfile%#$q"
FileRename,%IsoFolder%\%ISOfilenameWOextenstion%.ISO.gz,%IsoFolder%\%ISOfilenameWOextenstion%.gz
FileRename,%ISOFile%_GzipBackup,%ISOFile%
//-
Retrieve,FileSize,%IsoFolder%\%ISOfilenameWOextenstion%.gz,%sizeISO%
Echo,"ISO GZP Compressed - %sizeISO% bytes",Warn
StrFormat,Bytes,%sizeISO%
Echo,"ISO GZP Compressed - %sizeISO%",Warn
//-
End
End

[Compress]
//#1 Folder\Filename #2 FileExtension #3 FileTargetExtension
If,ExistFile,#1.#2,Begin
//-
If,ExistFile,#1.#3,ShellExecute,Hide,attrib.exe,"-R -S -A -H #$q#1.#3#$q"
If,ExistFile,#1.#3,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q#1.#3#$q&del /q /f /A:R /A:H /A:S /A:A #$q#1.#3#$q"
If,ExistFile,#1.#3,Echo,"File NOT DELETED",Warn
//-
If,Not,ExistFile,#1.#3,Begin
ShellExecute,Hide,attrib.exe,"-R -S -A -H #$q#1.#2#$q"
ShellExecute,Hide,makecab.exe,"/D CompressionType=LZX /D CompressionMemory=21 /D Cabinet=ON /D Compress=ON /D FolderSizeThreshold=5000000 #$q#1.#2#$q #$q#1.#3#$q"
//ShellExecute,Hide,makecab.exe,"#$q#1.#2#$q #$q#1.#3#$q"
If,ExistFile,#1.#3,Begin
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q#1.#2#$q&del /q /f /A:R /A:H /A:S /A:A #$q#1.#2#$q"
If,ExistFile,#1.#2,Echo,"File NOT DELETED",Warn
End
Else,Echo,"#1.#2 NOT Compressed to #1.#3",Warn
End
Else,Echo,"#1.#3 File NOT DELETED - NO Compression Made",Warn
End
Else,Echo,"#1.#2 NOT FOUND",Warn

