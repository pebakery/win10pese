//ShellExecute,Open,makecab.exe,"/D CompressionType=LZX /D CompressionMemory=21 /D Cabinet=ON /D Compress=ON /D FolderSizeThreshold=5000000 #$q%ISOfile%#$q #$q%RAMFolder%\I386\%ISOfilenameWOextenstion%.is_#$q"
