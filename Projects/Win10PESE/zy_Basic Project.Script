[P_HostOS_RC_Path_NoCarriage_CallExecCall]
//http://www.askvg.com/registry-tweak-to-add-copy-as-path-option-in-files-and-folders-context-menu-in-windows/
//Copy as Path Option_NoCarriage
//Very Useful with Call,Upload with Codebox ;)
RegWrite,HKLM,0x1,"Software\Classes\*\shell\Copy as Path ExecCall\command",,"cmd.exe /c (echo.|set /p=Call,ExecCall,#$q#$p1#$q)|clip"

[P_HostOS_RC_Path_NoCarriage_CallExecCall_R]
System,FileRedirect,OFF
ShellExecute,Hide,Reg.exe,"Delete #$qHKCR\*\shell\Copy as Path ExecCall#$q /f"
System,FileRedirect,ON

[Open_ProjectDir_P_B]
Call,OpenDir,%ProjectDir%

[Variables]

[Process]
//If,%Halt_CheckBox%,Equal,True,Halt,"Time to check"
//Call,FileDelete,%ProjectTemp%\Basic_PluginCache_Extract.ini
//Call,FileDelete,%ProjectTemp%\Components_PluginCache_Extract.ini
//Halt,"Time to check"
IniRead,%ScriptFile%,Main,Interface,%InterfaceRead%
If,%InterfaceRead%,Equal,Interface,Run,%ScriptFile%,Process_Main
If,%InterfaceRead%,Equal,Interface_Basic,Run,%ScriptFile%,Process_Main
If,%InterfaceRead%,Equal,Interface_GenaDev,Run,%ScriptFile%,Process_GenaDev_InterfaceCheck
If,%InterfaceRead%,Equal,Interface_Debug,Call,Echo,"Nothing to process Interface_Debug - Time to Check - Halt",Warn,,,,Halt

[Process_GenaDev_InterfaceCheck]
If,Not,%OSFamily%,Equal,NT5,Call,Echo,"Nothing to process Interface_GenaDev on %OSFamily%  - Time to Check - Halt",Warn,,,,Halt
If,%OSFamily%,Equal,NT5,Run,%ScriptFile%,Process_GenaDev

[Process_Main]
IniRead,%ScriptFile%,Main,Interface,%t%
If,%t%,Equal,Interface_Basic,Begin
Call,AddInterface,%ScriptFile%,Interface
Call,AddInterface,%ScriptFile%,Interface_Basic
End
IniRead,%ProjectDir%\Script.Project,Main,Title,%MainProjectTitle%
If,%MainProjectTitle%,Equal,Azin,Run,%ScriptFile%,Process_Azin
If,Not,%MainProjectTitle%,Equal,Azin,Run,%ScriptFile%,Process_Main_P

[Process_Azin]
Exec,"%ProjectDir%\Finals\yy_050_PostConfig_Azin.Script",Process
Exec,"%ProjectDir%\Finals\yy_060_CreateISO_Azin.Script",Process
//--
If,%VirtualTest_CheckBox%,Equal,True,Run,%ScriptFile%,Process_VirtualTest
//--
Halt,"Time to check"

[Process_Main_P]
//--
If,%BRC_Restore_RadioGroup%,Equal,1,Run,%ScriptFile%,BRB_Restore
If,%BRC_Restore_RadioGroup%,Equal,2,Run,%ScriptFile%,BRC_Restore
If,%BRC_Restore_RadioGroup%,Equal,3,Run,%ScriptFile%,BRC_Restore_zt
//--
If,%Auto_Plugins_Plugin_CheckBox%,Equal,True,Begin
If,ExistFile,%BaseDir%\Projects\MyPlugins\za_Plugins.Script,Call,ExecCall,%BaseDir%\Projects\MyPlugins\za_Plugins.Script,Process
End
//--
