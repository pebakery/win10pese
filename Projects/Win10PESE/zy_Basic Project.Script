[Process_GenaDev_GZP_ISO]
If,Not,ExistFile,%ISOfile%,Call,Echo,"Can NOT Find %ISOfile%#$x#$x  #$x#$xPress OK to continue.",Warn,,Message
Else,Begin
//-
StrFormat,Path,%IsoFile%,%IsoFolder%
StrFormat,CTrim,%IsoFolder%,\,%IsoFolder%
//-
//To test compressed GZ size - which one can also boot easily from grub4dos ;) ( Info: Cab Compress is better but requires special work to get it boot)
//Echo,"Gzip is compressing Please wait (It will take some time) ...."
//-
StrFormat,FileName,%ISOfile%,%ISOfilenameWextenstion%
StrFormat,RTrim,%ISOfilenameWextenstion%,4,%ISOfilenameWOextenstion%
//-
Call,FileDelete,%ISOFile%_GzipBackup
