[Interface_Prepare_Container_P_B]
IniWrite,%ScriptFile%,Main,Interface,Interface_Prepare_Container
System,RefreshInterface

[Variables]
%ProgramTitle%=qEmu-X
%ProgramEXE%=qemu-system-i386.exe
%ProgramFolder%=qEmuX
%WF%=%ProjectTemp%\%ProgramFolder%
%hddInclude%=Nop
%DiskBoot%=0
%FileContainer%=%FileContainer_x86%
%FileContainer_x64%=%BaseDir%\Projects\Yomi\VirtualTestY\qEmu_X_x64_File.Packed
%FileContainer_x86%=%BaseDir%\Projects\Yomi\VirtualTestY\qEmu_X_x86_File.Packed
%FileContainer_NT5_x86%=%BaseDir%\Projects\Yomi\VirtualTestY\qEmu_X_NT5x_x86_File.Packed
%FileContainer_NT5_x64%=%BaseDir%\Projects\Yomi\VirtualTestY\qEmu_X_NT5x_x64_File.Packed

[Process]
IniRead,%ScriptFile%,Main,Interface,%t%
If,%t%-,Equal,-,Set,%t%,Interface
If,Not,%t%,Equal,Interface,Call,AddInterface,%ScriptFile%,Interface
//-
Call,HostOSVersion,,1
Call,HostOSArch
//2016-10-16: New QEMU installers. Drop SDL and Windows XP support, use updated third party DLLs, support more QEMU features.
//If,%HostOSVersion1%,Smaller,6,Exit,"%ScriptTitle% only support Vista and later"
//--
If,Not,%Custom_CheckBox%,Equal,True,Begin
//---- FileContainer Check - Start
If,%HostOSVersion1%,Equal,5,Begin
If,%HostOSArch%,Equal,x86,Begin
Set,%FileContainer%,%FileContainer_NT5_x86%
If,Not,ExistFile,%FileContainer%,Begin
StrFormat,FileName,%FileContainer%,%FileContainer_FileName%
If,ExistFile,%ScriptDir%\%FileContainer_FileName%,Set,%FileContainer%,%ScriptDir%\%FileContainer_FileName%
End
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_NT5x_x86_P
End
//-
If,%HostOSArch%,Equal,x64,Begin
Set,%FileContainer%,%FileContainer_NT5_x64%
If,Not,ExistFile,%FileContainer%,Begin
StrFormat,FileName,%FileContainer%,%FileContainer_FileName%
If,ExistFile,%ScriptDir%\%FileContainer_FileName%,Set,%FileContainer%,%ScriptDir%\%FileContainer_FileName%
End
//x64 Fall Back to x86
If,Not,ExistFile,%FileContainer%,Begin
Set,%FileContainer%,%FileContainer_NT5_x86%
If,Not,ExistFile,%FileContainer%,Begin
StrFormat,FileName,%FileContainer%,%FileContainer_FileName%
If,ExistFile,%ScriptDir%\%FileContainer_FileName%,Set,%FileContainer%,%ScriptDir%\%FileContainer_FileName%
End
End
If,Not,ExistFile,%FileContainer%,Set,%FileContainer%,%FileContainer_NT5_x64%
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_NT5x_x64_P
End
//-
// All FallBack to x86
If,Not,%HostOSArch%,Equal,x86,Begin
If,Not,%HostOSArch%,Equal,x64,Begin
Set,%FileContainer%,%FileContainer_NT5_x86%
If,Not,ExistFile,%FileContainer%,Begin
StrFormat,FileName,%FileContainer%,%FileContainer_FileName%
If,ExistFile,%ScriptDir%\%FileContainer_FileName%,Set,%FileContainer%,%ScriptDir%\%FileContainer_FileName%
End
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_NT5x_x86_P
End
End
End
//---------------------------------
If,%HostOSVersion1%,Bigger,5,Begin
If,%HostOSArch%,Equal,x86,Begin
Set,%FileContainer%,%FileContainer_x86%
If,Not,ExistFile,%FileContainer%,Begin
StrFormat,FileName,%FileContainer%,%FileContainer_FileName%
If,ExistFile,%ScriptDir%\%FileContainer_FileName%,Set,%FileContainer%,%ScriptDir%\%FileContainer_FileName%
End
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_x86_P
End
//-
If,%HostOSArch%,Equal,x64,Begin
Set,%FileContainer%,%FileContainer_x64%
If,Not,ExistFile,%FileContainer%,Begin
StrFormat,FileName,%FileContainer%,%FileContainer_FileName%
If,ExistFile,%ScriptDir%\%FileContainer_FileName%,Set,%FileContainer%,%ScriptDir%\%FileContainer_FileName%
End
//x64 Fall Back to x86
If,Not,ExistFile,%FileContainer%,Begin
Set,%FileContainer%,%FileContainer_x86%
If,Not,ExistFile,%FileContainer%,Begin
StrFormat,FileName,%FileContainer%,%FileContainer_FileName%
If,ExistFile,%ScriptDir%\%FileContainer_FileName%,Set,%FileContainer%,%ScriptDir%\%FileContainer_FileName%
End
End
If,Not,ExistFile,%FileContainer%,Set,%FileContainer%,%FileContainer_x64%
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_x64_P
End
//-
// All FallBack to x86
If,Not,%HostOSArch%,Equal,x86,Begin
If,Not,%HostOSArch%,Equal,x64,Begin
Set,%FileContainer%,%FileContainer_x86%
If,Not,ExistFile,%FileContainer%,Begin
StrFormat,FileName,%FileContainer%,%FileContainer_FileName%
If,ExistFile,%ScriptDir%\%FileContainer_FileName%,Set,%FileContainer%,%ScriptDir%\%FileContainer_FileName%
End
If,Not,ExistFile,%FileContainer%,Run,%ScriptFile%,DownloadXXX_Container_x86_P
End
End
End
//-
//---- FileContainer Check - Finish
//--
//Debug ----- S
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu x86",Set,%FileContainer%,%FileContainer_x86%
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu x64",Set,%FileContainer%,%FileContainer_x64%
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu NT5 x64",Set,%FileContainer%,%FileContainer_NT5_x64%
If,%SelectContainer_ScrollBox%,Equal,"Force qEmu NT5 x86",Set,%FileContainer%,%FileContainer_NT5_x86%
//--
//Set,%FileContainer%,%FileContainer_x86%
//Set,%FileContainer%,%FileContainer_x64%
//Set,%FileContainer%,%FileContainer_NT5_x86%
//Set,%FileContainer%,%FileContainer_NT5_x64%
//Debug ----- F
//--
If,Not,ExistFile,%FileContainer%,Call,Echo,"%FileContainer% NOT FOUND",Warn,,Message,,Exit
//--
If,%FileContainer%,Equal,%FileContainer_x64%,Set,%WF%,%WF%_x64
If,%FileContainer%,Equal,%FileContainer_x86%,Set,%WF%,%WF%_x86
If,%FileContainer%,Equal,%FileContainer_NT5_x64%,Set,%WF%,%WF%_NT5_x64
If,%FileContainer%,Equal,%FileContainer_NT5_x86%,Set,%WF%,%WF%_NT5_x86
//--
End
Else,Begin
Set,%Custom_FileBoxV%,%Custom_FileBox%
StrFormat,Right,%Custom_FileBoxV%,1,%t%
If,%t%,Equal,\,StrFormat,RTrim,%Custom_FileBoxV%,1,%Custom_FileBoxV%
//-
If,%Custom_FileBoxV%-,Equal,-,Begin
Set,%Custom_FileBox%,%BaseDir%\Workbench\Common\qEmuX_ProvideFiles\qEmuX\,Permanent
Set,%Custom_FileBoxV%,%BaseDir%\Workbench\Common\qEmuX_ProvideFiles\qEmuX\
End
//-
Set,%WF%,%Custom_FileBoxV%
If,Not,%Custom_VirtualOS_ScrollBox%,Equal,Auto,Set,%ProgramEXE%,%Custom_VirtualOS_ScrollBox%
//-
//In case setup file extracted instead of installed better to check and if required add loaders.cache
If,ExistDir,%WF%\lib\gdk-pixbuf-2.0\2.10.0,Begin
If,Not,ExistFile,%WF%\lib\gdk-pixbuf-2.0\2.10.0\loaders.cache,Begin
//loaders.cache file created during setup !!! FallBack : Extract from current plugin:
ExtractFile,%ScriptFile%,Loaders_Cache,loaders.cache,%WF%\lib\gdk-pixbuf-2.0\2.10.0
End
End
//-
End
//-
Echo,"Processing %ProgramTitle%..."
//--
If,Not,%Custom_CheckBox%,Equal,True,Begin
If,Not,ExistFile,%WF%\%ProgramEXE%,Run,%ScriptFile%,Extract
End
//--
Set,%VirtualOS%,%VirtualOS_ScrollBox%
If,%Custom_CheckBox%,Equal,True,Set,%VirtualOS%,%Custom_VirtualOS_ScrollBox%
If,%VirtualOS%,Equal,Auto,Begin
If,%SourceArch%,Equal,x86,Set,%Exe%,qemu-system-i386.exe
If,%SourceArch%,Equal,x64,Set,%Exe%,qemu-system-x86_64.exe
End
Else,Set,%Exe%,%VirtualOS%
Echo,%Exe%
//-
Set,%t%,0
If,%Custom_CheckBox%,Equal,True,Begin
If,Not,ExistFile,%WF%\%ProgramEXE%,Set,%t%,1
End
If,%t%,Equal,1,Call,Echo,"Error: File Not Found:#$x#$x%WF%\%ProgramEXE%",Warn,,MessageError,10,Exit
//-
Set,%BootDrive_ScrollBoxV%,%BootDrive_ScrollBox%
//--
If,Not,ExistVar,%ISOfileVar%,Begin
Set,%ISOfileVar%,%ISOfile%
Set,%t%,""
IniRead,%ProjectDir%\script.project,IniVariables,ISOfileVar,%t%
If,Not,%t%,Equal,"",Set,%ISOfileVar%,%t%
End
//-
Set,%Ram%,%Ram_NumberBox%
//-
Set,%Ex%,Ex
If,%Pause_CheckBox%,Equal,True,Set,%Ex%,""
//-
If,%Hdd_Include_CheckBox%,Equal,True,Set,%hddInclude%,Yep
