If,%Ex%--,Equal,--,ShellExecute,Open,%WM_Exe%,%switches%#$s#$s#$q%VMXFile%#$q
If,%Ex%,Equal,Ex,ShellExecuteEx,Open,%WM_Exe%,%switches%#$s#$s#$q%VMXFile%#$q
Echo,ExitCode_%ExitCode%
