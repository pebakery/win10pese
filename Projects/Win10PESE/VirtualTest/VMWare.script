If,ExistFile,%VMXDir%\%tmp%.vmsd,FileDelete,%VMXDir%\%tmp%.vmsd
If,ExistFile,%VMXDir%\%tmp%.vmxf,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXDir%\%tmp%.vmxf#$q"
If,ExistFile,%VMXDir%\%tmp%.vmxf,FileDelete,%VMXDir%\%tmp%.vmxf
If,ExistFile,%VMXDir%\%tmp%.vmem,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXDir%\%tmp%.vmem#$q"
If,ExistFile,%VMXDir%\%tmp%.vmem,FileDelete,%VMXDir%\%tmp%.vmem
If,ExistFile,%VMXDir%\%tmp%.vmss,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXDir%\%tmp%.vmss#$q"
