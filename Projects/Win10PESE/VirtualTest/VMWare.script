[Process]
If,Not,ExistVar,%ISOfileVar%,Begin
Set,%ISOfileVar%,%ISOfile%
Set,%t%,""
IniRead,%ProjectDir%\script.project,IniVariables,ISOfileVar,%t%
If,Not,%t%,Equal,"",Set,%ISOfileVar%,%t%
End
If,Not,ExistFile,%ISOfileVar%,Message,"There is no ISO file to boot",Error,5
If,Not,ExistFile,%ISOfileVar%,Exit,"There is no ISO file to boot"
//--
Call,TargetVersion,,4x
//--
//JFX: Reply 1445 Normally on a Windows 10 with enabled Hyper-V, Virtualbox and VMWare notice this and don't start any VM.
//HYPERVISORLAUNCHTYPE=AUTO
IniRead,%ScriptFile%,Main,Title,%t%
Set,%SystemStartOptions%,-
If,ExistRegKey,HKLM,System\CurrentControlSet\Control,SystemStartOptions,RegRead,HKLM,System\CurrentControlSet\Control,SystemStartOptions,%SystemStartOptions%
StrFormat,Replace,%SystemStartOptions%,HYPERVISORLAUNCHTYPE=AUTO,-,%SystemStartOptionsTEST%
If,Not,%SystemStartOptions%,Equal,%SystemStartOptionsTEST%,Exit,"%t% DO NOT support 'Enabled Hyper-V'"
//--
Run,%ScriptFile%,HuntVMware
//--
//WS: VMware Workstation - vmware.exe
//PL: VMware Player - vmplayer.exe
//SV: VMware Server - vmware.exe or vmplayer.exe
If,Not,%PL%,Equal,--,Set,%WM_Exe%,%PL%
If,Not,%SV%,Equal,--,Set,%WM_Exe%,%SV%
If,Not,%WS%,Equal,--,Set,%WM_Exe%,%WS%
If,%Pl_Prior_CheckBox%,Equal,True,Begin
If,Not,%PL%,Equal,--,Set,%WM_Exe%,%PL%
End
StrFormat,Path,%WM_Exe%,%WM_Path%
StrFormat,Right,%WM_Path%,1,%t%
If,%t%,Equal,\,StrFormat,RTrim,%WM_Path%,1,%WM_Path%
//--
//Retrieve,FileVersion,%WM_Exe%,%VMw_Version% winbuilder Bug - DO NOT USE - Kare Win7 Host x64 ) can not read version on hostx64 at c:\program files\vmware\vmware player\vmplayer.exe
Call,Version,%WM_Exe%,VMw_Version
Echo,%VMw_Version%
StrFormat,Split,%VMw_Version%,.,1,%VMw_Version1%
//--
//----------Rock'n Roll Time
//Delete files created/used by vmware on previous session - fxscrpt added
//--
//Both %VMXDir%\nvram and %VMXFile% are critical
System,FileRedirect,OFF
If,ExistFile,%VMXDir%\nvram,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXDir%\nvram#$q&del /q /f /A:R /A:H /A:S /A:A #$q%VMXDir%\nvram#$q"
If,ExistFile,%VMXDir%\nvram,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXDir%\nvram#$q&del /q /f /A:R /A:H /A:S /A:A #$q%VMXDir%\nvram#$q"
If,ExistFile,%VMXDir%\nvram,Begin
If,ExistFile,%VMXFile%,Begin
//If,ExistFile,%VMXFile%,Begi-n
//If,ExistFile,%WM_Path%\vmrun.exe,ShellExecute,Hide,cmd.exe,"/C vmrun.exe stop #$q%VMXFile%#$q",%WM_Path%
//En-d
//If,ExistFile,%WM_Path%\vmrun.exe,ShellExecute,Open,cmd.exe,"/K vmrun.exe stop #$q%VMXFile%#$q",%WM_Path%
If,ExistFile,%WM_Path%\vmrun.exe,ShellExecute,Hide,vmrun.exe,"stop #$q%VMXFile%#$q",%WM_Path%
End
End
//vmrun.exe send shutdown command and exits before virtual machine shut down (better than nothing ;) )
If,ExistFile,%VMXDir%\nvram,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXDir%\nvram#$q&del /q /f /A:R /A:H /A:S /A:A #$q%VMXDir%\nvram#$q"
If,ExistFile,%VMXDir%\nvram,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXDir%\nvram#$q&del /q /f /A:R /A:H /A:S /A:A #$q%VMXDir%\nvram#$q"
If,ExistFile,%VMXDir%\nvram,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXDir%\nvram#$q&del /q /f /A:R /A:H /A:S /A:A #$q%VMXDir%\nvram#$q"
If,ExistFile,%VMXFile%,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXFile%#$q&del /q /f /A:R /A:H /A:S /A:A #$q%VMXFile%#$q"
If,ExistFile,%VMXFile%,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%VMXFile%#$q&del /q /f /A:R /A:H /A:S /A:A #$q%VMXFile%#$q"
