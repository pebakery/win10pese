[Ethernet0]
//All info here from sanbarrow page - For more info check sanbarrow page : http://sanbarrow.com/vmx/vmx-network.html
//--
TxtAddLine,%VMXFile%,"Ethernet0.present=#$qTRUE#$q",Append
//TxtAddLine,%VMXFile%,"ethernet0.startConnected=#$qtrue#$q",Append
//--
//bridged:  to an automatically assigned physical nic
//          WARNING: this is unreliable on hosts with more than one physical nic.
//hostonly: You can use the VMware-builtin DHCP-service to auto-configure guests that use this type of connection.
//          WARNING: this is unreliable on hosts that don't use defaults for the vmnet-mapping.
//          NOTE: this is the same as ethernet0.connectionType="vmnet1"
//nat:      Connected to the virtal adapter of the host named "nat"
//          You can use the VMware-builtin DHCP-service to auto-configure guests that use this type of connection.
//          Guests configured this way can also use the hosts internet connection via the VMware NAT service.
//          WARNING: this is unreliable on hosts that don't use defaults for the vmnet-mapping.
//          Note: this is the same as ethernet0.connectionType="vmnet8"
//Set,%cType%,bridged
Set,%cType%,nat
If,Not,%Ethernet0_md_ScrollBox%,Equal,Auto,Set,%cType%,%Ethernet0_md_ScrollBox%
//If,%Ethernet0_md_ScrollBox%%OSFamily%,Equal,Auto98,Set,%cType%,nat
//If,%Ethernet0_md_ScrollBox%%OSFamily%,Equal,AutoNT6,Set,%cType%,bridged
//If,%Ethernet0_md_ScrollBox%%OSFamily%,Equal,AutoNT6,Set,%cType%,nat
//If,%Ethernet0_md_ScrollBox%%OSFamily%,Equal,AutoNT6,Set,%cType%,nat
TxtAddLine,%VMXFile%,"Ethernet0.connectionType=#$q%cType%#$q",Append
//--
TxtAddLine,%VMXFile%,"Ethernet0.addressType=#$qgenerated#$q",Append
//--
//vlance : AMD PCnet32 network-card  --> Most older OS have drivers for this nic
//vmxnet : VMXnet network-card       --> You can only use this nic if you install the VMware-tools inside the guest
//e1000  : Intel E1000 network-card  --> Modern OS should have drivers for this nic.
//         NOTE: VMware usually assigns this nic to 64bit-guests - but it also works with 32-bit guests
//ps: NT6 projects prefer to use single e1000 for all, others leave this entry to default....
If,Not,%Ethernet0_vd_ScrollBox%,Equal,Default,Begin
If,Not,%Ethernet0_vd_ScrollBox%,Equal,Auto,TxtAddLine,%VMXFile%,"Ethernet0.virtualDev=#$q%Ethernet0_vd_ScrollBox%#$q",Append
If,%Ethernet0_vd_ScrollBox%,Equal,Auto,Begin
If,%OSFamily%,Equal,NT6,TxtAddLine,%VMXFile%,"Ethernet0.virtualDev=#$qe1000#$q",Append
If,%OSFamily%,Equal,NT5,Begin
If,Not,%SourceArch%,Equal,x86,TxtAddLine,%VMXFile%,"Ethernet0.virtualDev=#$qe1000#$q",Append
If,%SourceArch%,Equal,x86,TxtAddLine,%VMXFile%,"Ethernet0.virtualDev=#$qvlance#$q",Append
End
End
End

