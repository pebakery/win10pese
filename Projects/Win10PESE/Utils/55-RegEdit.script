[Backup_Hives]
System,Cursor,Wait
Run,%ScriptFile%,Where_Are_Hives
//
Echo,"Backing up registry hives..."
If,Not,ExistFile,%RegSoftware%,Message,"Target hive Folder Not Exists",Warning
//If,Not,ExistFile,%RegSoftware%,Halt,"Target hive Folder Not Exists"
If,ExistFile,%RegSystem%,FileCopy,%RegSystem%,%RegSystem%.bak
If,ExistFile,%RegDefault%,FileCopy,%RegDefault%,%RegDefault%.bak
If,ExistFile,%RegSoftware%,FileCopy,%RegSoftware%,%RegSoftware%.bak
If,ExistFile,%RegComponents%,FileCopy,%RegComponents%,%RegComponents%.bak
If,ExistFile,%RegDrivers%,FileCopy,%RegDrivers%,%RegDrivers%.bak
If,ExistFile,%RegUser%,FileCopy,%RegUser%,%RegUser%.bak
If,ExistFile,%RegSam%,FileCopy,%RegSam%,%RegSam%.bak
If,ExistFile,%RegSecurity%,FileCopy,%RegSecurity%,%RegSecurity%.bak
System,Cursor,Normal

[Restore_Hives]
System,Cursor,Wait
Run,%ScriptFile%,Where_Are_Hives
//
Echo,"Restoring up registry hives..."
If,Not,ExistFile,%RegSoftware%,Message,"Target hive Folder Not Exists",Warning
//If,Not,ExistFile,%RegSoftware%,Halt,"Target hive Folder Not Exists"
If,ExistFile,%RegSoftware%,If,Not,ExistFile,%RegSoftware%.bak,Message,"You must backup first",Warning
//If,Not,ExistFile,%RegSoftware%.bak,Halt,"You must backup first"
If,ExistFile,%RegSystem%,FileCopy,%RegSystem%.bak,%RegSystem%
If,ExistFile,%RegDefault%,FileCopy,%RegDefault%.bak,%RegDefault%
If,ExistFile,%RegSoftware%,FileCopy,%RegSoftware%.bak,%RegSoftware%
If,ExistFile,%RegComponents%,FileCopy,%RegComponents%.bak,%RegComponents%
If,ExistFile,%RegDrivers%,FileCopy,%RegDrivers%.bak,%RegDrivers%
If,ExistFile,%RegUser%,FileCopy,%RegUser%.bak,%RegUser%
If,ExistFile,%RegSam%,FileCopy,%RegSam%.bak,%RegSam%
If,ExistFile,%RegSecurity%,FileCopy,%RegSecurity%.bak,%RegSecurity%
System,Cursor,Normal

[Delete_Backup_Hives]
System,Cursor,Wait
Run,%ScriptFile%,Where_Are_Hives
//
If,ExistFile,%RegSystem%.bak,FileDelete,%RegSystem%.bak
If,ExistFile,%RegDefault%.bak,FileDelete,%RegDefault%.bak
If,ExistFile,%RegSoftware%.bak,FileDelete,%RegSoftware%.bak
If,ExistFile,%RegComponents%.bak,FileDelete,%RegComponents%.bak
If,ExistFile,%RegDrivers%.bak,FileDelete,%RegDrivers%.bak
If,ExistFile,%RegUser%.bak,FileDelete,%RegUser%.bak
If,ExistFile,%RegSam%.bak,FileDelete,%RegSam%.bak
If,ExistFile,%RegSecurity%.bak,FileDelete,%RegSecurity%.bak
System,Cursor,Normal

[Where_Are_Hives]
If,Not,ExistFile,%RegSystem%,If,%Mount%,Equal,%BaseDir%\Mount\%ProjectName%\Target,Begin
If,ExistFile,%Mount%\Windows\System32\config\system,Set,%RegSystem%,%Mount%\Windows\System32\config\system
If,ExistFile,%Mount%\Windows\System32\config\default,Set,%RegDefault%,%Mount%\Windows\System32\config\default
If,ExistFile,%Mount%\Windows\System32\config\software,Set,%RegSoftware%,%Mount%\Windows\System32\config\software
If,ExistFile,%Mount%\Windows\System32\config\COMPONENTS,Set,%RegComponents%,%Mount%\Windows\System32\config\COMPONENTS
If,ExistFile,%Mount%\Windows\System32\config\DRIVERS,Set,%RegDrivers%,%Mount%\Windows\System32\config\DRIVERS
If,ExistFile,%Mount%\Users\Default\NTUSER.DAT,Set,%RegUser%,%Mount%\Users\Default\NTUSER.DAT
Else,If,ExistFile,%Mount%\Windows\System32\config\default,Set,%RegUser%,%Mount%\Windows\System32\config\default
If,ExistFile,%Mount%\Windows\System32\config\sam,Set,%RegSam%,%Mount%\Windows\System32\config\sam
If,ExistFile,%Mount%\Windows\System32\config\security,Set,%RegSecurity%,%Mount%\Windows\System32\config\security
End
If,%BuildClass%,Equal,98,Begin
If,Not,ExistVar,%RegSystem%,Set,%RegSystem%,%app_tmp%\setupreg.hiv
If,Not,ExistVar,%RegDefault%,Set,%RegDefault%,%app_tmp%\Config\default
If,Not,ExistVar,%RegSoftware%,Set,%RegSoftware%,%app_tmp%\Config\software
If,Not,ExistVar,%RegComponents%,Set,%RegComponents%,%app_tmp%\Config\COMPONENTS
If,Not,ExistVar,%RegDrivers%,Set,%RegDrivers%,%app_tmp%\Config\DRIVERS
If,Not,ExistVar,%RegUser%,Set,%RegUser%,%app_tmp%\Config\default
If,Not,ExistVar,%RegSam%,Set,%RegSam%,%app_tmp%\Config\sam
End
If,Not,ExistVar,%RegSystem%,Set,%RegSystem%,%Target_Sys%\config\system
If,Not,ExistVar,%RegDefault%,Set,%RegDefault%,%Target_Sys%\config\default
If,Not,ExistVar,%RegSoftware%,Set,%RegSoftware%,%Target_Sys%\config\software
If,Not,ExistVar,%RegComponents%,Set,%RegComponents%,%Target_Sys%\config\COMPONENTS
If,Not,ExistVar,%RegDrivers%,Set,%RegDrivers%,%Target_Sys%\Config\DRIVERS
If,Not,ExistVar,%RegUser%,Begin
If,ExistFile,%TargetDir%\Users\Default\NTUSER.DAT,Set,%RegUser%,%TargetDir%\Users\Default\NTUSER.DAT
Else,Set,%RegUser%,%Target_Sys%\config\default
End
If,Not,ExistVar,%RegSam%,Set,%RegSam%,%Target_Sys%\config\sam
If,Not,ExistVar,%RegSecurity%,Set,%RegSecurity%,%Target_Sys%\config\security

[BackupHives]
Echo,"Backing up registry hives..."
If,ExistFile,%RegSystem%,FileCopy,%RegSystem%,%RegSystem%.bak
If,ExistFile,%RegDefault%,FileCopy,%RegDefault%,%RegDefault%.bak
If,ExistFile,%RegSoftware%,FileCopy,%RegSoftware%,%RegSoftware%.bak
If,ExistFile,%RegComponents%,FileCopy,%RegComponents%,%RegComponents%.bak
If,ExistFile,%RegDrivers%,FileCopy,%RegDrivers%,%RegDrivers%.bak
If,ExistFile,%RegUser%,FileCopy,%RegUser%,%RegUser%.bak
If,ExistFile,%RegSam%,FileCopy,%RegSam%,%RegSam%.bak
If,ExistFile,%RegSecurity%,FileCopy,%RegSecurity%,%RegSecurity%.bak

[TargetMount]
Echo,"Starting Mount process..."
//-
IniRead,%ProjectDir%\script.project,IniVariables,WIMTool,%WIMTool%
If,%WIMTool%,Equal,ImageX,IniRead,%ProjectDir%\script.project,IniVariables,WIMToolFile,%IMAGEX%
If,%WIMTool%,Equal,WimUtil,IniRead,%ProjectDir%\script.project,IniVariables,WIMToolFile,%WIMUTIL%
//-
If,%WIMTool%,Equal,ImageX,If,Not,ExistFile,%IMAGEX%,Run,%ScriptFile%,ImageXNotFound
If,%WIMTool%,Equal,WimUtil,If,Not,ExistFile,%WIMUTIL%,Run,%ScriptFile%,WimUtilNotFound
//--
If,ExistFile,%BootAF%,Set,%MountF%,%BootAF%
If,ExistFile,%BootVF%,Set,%MountF%,%BootVF%
If,ExistFile,%BootLF%,Set,%MountF%,%BootLF%
If,Not,ExistFile,%MountF%,Message,"Wim File not found",Error
If,Not,ExistFile,%MountF%,Halt,"Wim File not found"
//--
If,Not,ExistDir,%Mount%,DirMake,%Mount%
If,%WIMTool%,Equal,ImageX,Begin
StrFormat,Path,%IMAGEX%,%IMAGEXPath%
ShellExecute,Hide,%IMAGEX%,"/mountrw #$q%MountF%#$q 1 #$q%Mount%#$q",%IMAGEXPath%
End
If,%WIMTool%,Equal,WimUtil,Begin
StrFormat,Path,%WIMUTIL%,%WIMUTILPath%
ShellExecute,Hide,%WIMUTIL%,"/mount #$q%MountF%#$q 1 #$q%Mount%#$q /RW",%WIMUTILPath%
End
//--
If,ExistFile,%Mount%\Windows\System32\ntdll.dll,Echo,"Mount successful :)"
If,ExistFile,%Mount%\Windows\System32\ntdll.dll,Echo,"Mount successful :)"
If,ExistFile,%Mount%\Windows\System32\ntdll.dll,Message,"Mount successful :)",Information,%TSuccess%
If,ExistFile,%Mount%\Windows\System32\ntdll.dll,Set,%mountmade%,YES
If,Not,ExistFile,%Mount%\Windows\System32\ntdll.dll,Message,"Mount NOT successful :(",Error
If,Not,ExistFile,%Mount%\Windows\System32\ntdll.dll,Echo,"Mount NOT successful :(",Warn
If,Not,ExistFile,%Mount%\Windows\System32\ntdll.dll,Halt,"Mount NOT successful :("

[ImageXNotFound]
Message,"Image Tools ImageX was not found. Imagex.exe REQUIRED to mount !!!#$x#$xExiting...",Error
Echo,"Image Tools ImageX was not found. Imagex.exe REQUIRED to mount !!!",Warn
Halt,"Image Tools ImageX was not found. Imagex.exe REQUIRED to mount. Exiting..."

[WimUtilNotFound]
Message,"Image Tools WimUtil was not found. WimUtil.exe REQUIRED to mount !!!#$x#$xExiting...",Error
Echo,"Image Tools WimUtil was not found. WimUtil.exe REQUIRED to mount !!!",Warn
Halt,"Image Tools WimUtil was not found. WimUtil.exe REQUIRED to mount. Exiting..."

[TargetUnMount]
Echo,"Starting Unmount process..."
If,%WIMTool%,Equal,ImageX,Begin
ShellExecute,Hide,%IMAGEX%,"/unmount /commit #$q%Mount%#$q",%IMAGEXPath%
End
If,%WIMTool%,Equal,WimUtil,Begin
ShellExecute,Hide,%WIMUTIL%,"/unmount #$q%Mount%#$q /Commit /nowarn",%WIMUTILPath%
End
//--
IF,ExistFile,%Mount%\Windows\System32\config\system,Run,%ScriptFile%,TargetUnMount_Error
Else,Begin
Message,"Unmount Successful :)",Information,%TUnSuccess%
Echo,"Unmount Successful :)"
End
