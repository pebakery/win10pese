If,ExistFile,%RegSoftware%,Run,%ScriptFile%,LoadHive2_Reg
Else,Run,%ScriptFile%,LoadHive2PreMount

[LoadHive2PreMount]
Run,%ScriptFile%,TargetMount
Run,%ScriptFile%,LoadHive2_Reg

[LoadHive2_Reg]
Run,%ScriptFile%,Where_Are_Hives
//
If,%CB_SystemHive%,Equal,True,If,ExistFile,%RegSystem%,RegHiveLoad,%HSys%,%RegSystem%
If,%CB_DefaultHive%,Equal,True,If,ExistFile,%RegDefault%,RegHiveLoad,%HDef%,%RegDefault%
If,%CB_SoftwareHive%,Equal,True,If,ExistFile,%RegSoftware%,RegHiveLoad,%HSof%,%RegSoftware%
If,%CB_ComponentsHive%,Equal,True,If,ExistFile,%RegComponents%,RegHiveLoad,%HCom%,%RegComponents%
If,%CB_Drivers%,Equal,True,If,ExistFile,%RegDrivers%,RegHiveLoad,%HDrv%,%RegDrivers%
If,%CB_User%,Equal,True,If,ExistFile,%RegUser%,Begin
If,%RegDefault%,Equal,%RegUser%,If,Not,%CB_DefaultHive%,Equal,True,RegHiveLoad,%HUse%,%RegUser%
If,Not,%RegDefault%,Equal,%RegUser%,RegHiveLoad,%HUse%,%RegUser%
End
If,%CB_Sam%,Equal,True,If,ExistFile,%RegSam%,RegHiveLoad,%HSam%,%RegSam%
If,%CB_Security%,Equal,True,If,ExistFile,%RegSam%,RegHiveLoad,%HSec%,%RegSecurity%

[UnloadHive]
Echo,"Unloading registry hives..."
If,%CB_SystemHive%,Equal,True,If,ExistFile,%RegSystem%,RegHiveUnLoad,%HSys%
If,%CB_DefaultHive%,Equal,True,If,ExistFile,%RegDefault%,RegHiveUnLoad,%HDef%
If,%CB_SoftwareHive%,Equal,True,If,ExistFile,%RegSoftware%,RegHiveUnLoad,%HSof%
If,%CB_ComponentsHive%,Equal,True,If,ExistFile,%RegComponents%,RegHiveUnLoad,%HCom%
If,%CB_Drivers%,Equal,True,If,ExistFile,%RegDrivers%,RegHiveUnLoad,%HDrv%
