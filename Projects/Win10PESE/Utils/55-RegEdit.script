[Process]
Echo,"You can't run this script directly!"

[EditHive]
Run,%ScriptFile%,LoadHive%BuildClass%
Echo,"Regedit is running. Please close Regedit to continue."
Call,StartWait,,regedit.exe
Run,%ScriptFile%,UnloadHive
If,%mountmade%,Equal,YES,Run,%ScriptFile%,TargetUnMount

[LoadHive%BuildClass%]
If,%OSFamily%,Equal,NT5,Run,%ScriptFile%,LoadHive1
Else,Run,%ScriptFile%,LoadHive2

[LoadHive98]
//Windows 95, 98, and Me
//The registry files are named USER.DAT and SYSTEM.DAT are
//stored in the %WINDIR% directory.
//In Windows Me, Classes.dat was added.
//Also, each user profile (if profiles are enabled) has its own USER.DAT in profile's directory.
//--
If,Not,%HiveName_ScrollBox%,Equal,WB-X,Run,%ScriptFile%,HiveName_%HiveName_ScrollBox%
//
//-After Build
//%APP_TMP%=%ProjectTemp%\APP_TMP
//--%target_win%=%target_root%\LiveW98
//--SYSTEM.DAT
//--USER.DAT
//--%Target_Sys%=%target_root%\LiveW98\System
//--\Config
//--default
//--software
//-During Build:
//--%app_tmp%\setupreg.hiv (not in project but in capi by BeatZero)
//--%app_tmp%\Config\default
//--%app_tmp%\Config\software
//--%app_tmp%\Config\COMPONENTS (not in project but in capi by BeatZero)
//
If,Not,ExistVar,%RegSystem%,Set,%RegSystem%,%app_tmp%\setupreg.hiv
If,Not,ExistVar,%RegDefault%,Set,%RegDefault%,%app_tmp%\Config\default
If,Not,ExistVar,%RegSoftware%,Set,%RegSoftware%,%app_tmp%\Config\software
If,Not,ExistVar,%RegComponents%,Set,%RegComponents%,%app_tmp%\Config\COMPONENTS
If,Not,ExistVar,%RegDrivers%,Set,%RegComponents%,%app_tmp%\Config\DRIVERS
If,Not,ExistVar,%RegUser%,Set,%RegUser%,%app_tmp%\Config\default
If,Not,ExistVar,%RegSam%,Set,%RegSam%,%app_tmp%\Config\sam
//
If,%CB_BackupHives%,Equal,True,Run,%ScriptFile%,BackupHives
//
If,%CB_SystemHive%,Equal,True,If,ExistFile,%RegSystem%,RegHiveLoad,%HSys%,%RegSystem%
If,%CB_DefaultHive%,Equal,True,If,ExistFile,%RegDefault%,RegHiveLoad,%HDef%,%RegDefault%
If,%CB_SoftwareHive%,Equal,True,If,ExistFile,%RegSoftware%,RegHiveLoad,%HSof%,%RegSoftware%
If,%CB_ComponentsHive%,Equal,True,If,ExistFile,%RegComponents%,RegHiveLoad,%HCom%,%RegComponents%
If,%CB_Drivers%,Equal,True,If,ExistFile,%RegDrivers%,RegHiveLoad,%HDrv%,%RegDrivers%
If,%CB_User%,Equal,True,If,ExistFile,%RegUser%,Begin
If,%RegDefault%,Equal,%RegUser%,If,Not,%CB_DefaultHive%,Equal,True,RegHiveLoad,%HUse%,%RegUser%
If,Not,%RegDefault%,Equal,%RegUser%,RegHiveLoad,%HUse%,%RegUser%
End
If,%CB_Sam%,Equal,True,If,ExistFile,%RegSam%,RegHiveLoad,%HSam%,%RegSam%
If,%CB_Security%,Equal,True,If,ExistFile,%RegSam%,RegHiveLoad,%HSec%,%RegSecurity%

[LoadHive1]
If,Not,%HiveName_ScrollBox%,Equal,WB-X,Run,%ScriptFile%,HiveName_%HiveName_ScrollBox%
//
If,Not,ExistVar,%RegSystem%,Set,%RegSystem%,%Target_Sys%\SETUPREG.HIV
If,Not,ExistVar,%RegDefault%,Set,%RegDefault%,%Target_Sys%\config\default
If,Not,ExistVar,%RegSoftware%,Set,%RegSoftware%,%Target_Sys%\config\software
If,Not,ExistVar,%RegComponents%,Set,%RegComponents%,%Target_Sys%\config\COMPONENTS
If,Not,ExistVar,%RegDrivers%,Set,%RegDrivers%,%Target_Sys%\config\DRIVERS
If,Not,ExistVar,%RegUser%,Set,%RegUser%,%Target_Sys%\config\default
If,Not,ExistVar,%RegSam%,Set,%RegSam%,%Target_Sys%\config\sam
If,Not,ExistVar,%RegSecurity%,Set,%RegSecurity%,%Target_Sys%\config\security
//
If,%CB_BackupHives%,Equal,True,Run,%ScriptFile%,BackupHives
//
If,%CB_SystemHive%,Equal,True,If,ExistFile,%RegSystem%,RegHiveLoad,%HSys%,%RegSystem%
If,%CB_DefaultHive%,Equal,True,If,ExistFile,%RegDefault%,RegHiveLoad,%HDef%,%RegDefault%
If,%CB_SoftwareHive%,Equal,True,If,ExistFile,%RegSoftware%,RegHiveLoad,%HSof%,%RegSoftware%
If,%CB_ComponentsHive%,Equal,True,If,ExistFile,%RegComponents%,RegHiveLoad,%HCom%,%RegComponents%
If,%CB_Drivers%,Equal,True,If,ExistFile,%RegDrivers%,RegHiveLoad,%HDrv%,%RegDrivers%
If,%CB_User%,Equal,True,If,ExistFile,%RegUser%,Begin
If,%RegDefault%,Equal,%RegUser%,If,Not,%CB_DefaultHive%,Equal,True,RegHiveLoad,%HUse%,%RegUser%
If,Not,%RegDefault%,Equal,%RegUser%,RegHiveLoad,%HUse%,%RegUser%
End
If,%CB_Sam%,Equal,True,If,ExistFile,%RegSam%,RegHiveLoad,%HSam%,%RegSam%
If,%CB_Security%,Equal,True,If,ExistFile,%RegSam%,RegHiveLoad,%HSec%,%RegSecurity%

[LoadHive3]
Run,%ScriptFile%,LoadHive2

[LoadHive2]
If,Not,%HiveName_ScrollBox%,Equal,WB-X,Run,%ScriptFile%,HiveName_%HiveName_ScrollBox%
//
If,Not,ExistVar,%RegSoftware%,If,ExistFile,%Target_Sys%\config\software,Set,%RegSoftware%,%Target_Sys%\config\software
