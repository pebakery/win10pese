[Read_IE_Proxy_Reg_P_B]
//"IE Settings" Default GUI:
//"IE Settings" do not have option for UserName Password seperately:
//Run,%ScriptFile%,UNPW_Each_Set_Disable_B
//"IE Settings" do not have option for ALL UserName Password seperately:
//Run,%ScriptFile%,UNPW_ALL_Set_Disable_B
//--
//Better set back to default settings when importing from IE
Run,%ScriptFile%,Default_Plugin_P
//--
Set,%t%,""
If,ExistRegKey,HKCU,"Software\Microsoft\Windows\CurrentVersion\Internet Settings",ProxyEnable,Begin
RegRead,HKCU,"Software\Microsoft\Windows\CurrentVersion\Internet Settings",ProxyEnable,%t%
If,%t%,Equal,1,Set,%ProxyEnable_CheckBox%,True,Permanent
Else,Set,%ProxyEnable_CheckBox%,False,Permanent
End
Else,Set,%ProxyEnable_CheckBox%,False,Permanent
//--
Set,%t%,""
If,ExistRegKey,HKCU,"Software\Microsoft\Windows\CurrentVersion\Internet Settings",ProxyServer,Begin
RegRead,HKCU,"Software\Microsoft\Windows\CurrentVersion\Internet Settings",ProxyServer,%t%
If,Not,%t%-,Equal,-,Begin
//If there is equal sign inside value It means "Use the same proxy server for all protocols" Disabled -> different proxy server for different protocols.
//eg. http=192.168.211.131:8082;https=192.168.211.132:8082;ftp=192.168.211.133:8082;socks=192.168.211.134
//all values splitted with ";"
//maximum 4 values
StrFormat,Replace,%t%,=,|,%t2%
If,%t%,Equal,%t2%,Begin
Set,%Proxy_TextBox%,%t%,Permanent
Run,%ScriptFile%,Proxy_All_Same_Set_Enable_B
End
Else,Begin
//--
//Disable "Use the same proxy server for all protocols:"
Run,%ScriptFile%,Proxy_All_Same_Set_Disable_B
//--
StrFormat,Split,%t%,;,0,%TotalValues%
Echo,%TotalValues%
//-
If,0,Smaller,%TotalValues%,Begin
StrFormat,Split,%t%,;,1,%t2%
If,Not,%t2%-,Equal,-,Begin
StrFormat,Split,%t2%,=,1,%t3%
StrFormat,Split,%t2%,=,2,%t4%
If,Not,%t4%-,Equal,-,Begin
If,%t3%,Equal,http,Set,%Proxy_Http_TextBox%,%t4%,Permanent
If,%t3%,Equal,https,Set,%Proxy_Https_TextBox%,%t4%,Permanent
If,%t3%,Equal,ftp,Set,%Proxy_Ftp_TextBox%,%t4%,Permanent
//If,%t3%,Equal,socks,For now no socks support ? Requires FeedBack
End
End
End
//-
If,1,Smaller,%TotalValues%,Begin
StrFormat,Split,%t%,;,2,%t2%
If,Not,%t2%-,Equal,-,Begin
StrFormat,Split,%t2%,=,1,%t3%
StrFormat,Split,%t2%,=,2,%t4%
If,Not,%t4%-,Equal,-,Begin
If,%t3%,Equal,http,Set,%Proxy_Http_TextBox%,%t4%,Permanent
If,%t3%,Equal,https,Set,%Proxy_Https_TextBox%,%t4%,Permanent
If,%t3%,Equal,ftp,Set,%Proxy_Ftp_TextBox%,%t4%,Permanent
//If,%t3%,Equal,socks,For now no socks support ? Requires FeedBack
End
End
End
//-
If,2,Smaller,%TotalValues%,Begin
StrFormat,Split,%t%,;,3,%t2%
If,Not,%t2%-,Equal,-,Begin
StrFormat,Split,%t2%,=,1,%t3%
StrFormat,Split,%t2%,=,2,%t4%
If,Not,%t4%-,Equal,-,Begin
If,%t3%,Equal,http,Set,%Proxy_Http_TextBox%,%t4%,Permanent
If,%t3%,Equal,https,Set,%Proxy_Https_TextBox%,%t4%,Permanent
If,%t3%,Equal,ftp,Set,%Proxy_Ftp_TextBox%,%t4%,Permanent
//If,%t3%,Equal,socks,For now no socks support ? Requires FeedBack
End
End
End
//-
If,3,Smaller,%TotalValues%,Begin
StrFormat,Split,%t%,;,4,%t2%
If,Not,%t2%-,Equal,-,Begin
StrFormat,Split,%t2%,=,1,%t3%
StrFormat,Split,%t2%,=,2,%t4%
If,Not,%t4%-,Equal,-,Begin
If,%t3%,Equal,http,Set,%Proxy_Http_TextBox%,%t4%,Permanent
If,%t3%,Equal,https,Set,%Proxy_Https_TextBox%,%t4%,Permanent
If,%t3%,Equal,ftp,Set,%Proxy_Ftp_TextBox%,%t4%,Permanent
//If,%t3%,Equal,socks,For now no socks support ? Requires FeedBack
End
End
End
//-
End
End
End
//-
//When "Bypass proxy server for local addresses" enabled ProxyOverride exists with value: <local>
//When "Do not use proxy server for addresses begining with:" box filled with values, values added to ProxyOverride (seperated by ;)
//<local> always last value
//eg.
//ProxyOverride
//blabla;oboobo;<local>
//
//For now we do not know how to add <local> setting to aria2 or wget so we delete <local>
Set,%t%,""
If,ExistRegKey,HKCU,"Software\Microsoft\Windows\CurrentVersion\Internet Settings",ProxyOverride,Begin
RegRead,HKCU,"Software\Microsoft\Windows\CurrentVersion\Internet Settings",ProxyOverride,%t%
If,Not,%t%-,Equal,-,Begin
StrFormat,Replace,%t%,;<local>,,%t2%
If,Not,%t2%,Equal,<local>,Begin
Set,%NoProxy_TextBox%,%t2%,Permanent
End
End
End
//--
If,%Read_IE_Proxy_Reg_Plus_SaveProxy_CheckBox%,Equal,True,Run,%ScriptFile%,Proxy_Ini_Save_P_B
Run,%ScriptFile%,Check_Enable_Proxy_P
System,RefreshInterface

