[Proxy_Ini_Save_P_B]
Run,%ScriptFile%,Proxy_Ini_Save_P
Run,%ScriptFile%,Check_Enable_Proxy_P

[Proxy_Ini_Save_P]
If,Not,ExistFile,%ProxyIni%,Run,%ScriptFile%,Proxy_Ini_New
//-
IniWrite,%ProxyIni%,ProxyInfo,ProxyEnabled,%ProxyEnable_CheckBox%
//-
IniWrite,%ProxyIni%,ProxyInfo,Address,%Proxy_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,UNPW_ALL,%UNPW_ALL_CheckBox%
IniWrite,%ProxyIni%,ProxyInfo,UserName_All,%UserName_All_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Password_All,%Password_All_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,BypassLocal,%BypassLocal_CheckBox%
IniWrite,%ProxyIni%,ProxyInfo,NoProxy_addresses_begining_with,%NoProxy_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Use_Same_Proxy_For_All_Protocols,%Proxy_All_Same_CheckBox%
IniWrite,%ProxyIni%,ProxyInfo,UNPW_Each,%UNPW_Each_CheckBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Http,%Proxy_Http_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Http_User,%Proxy_Http_User_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Http_Password,%Proxy_Http_Password_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Https,%Proxy_Https_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Https_User,%Proxy_Https_User_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Https_Password,%Proxy_Https_Password_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Ftp,%Proxy_Ftp_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Ftp_User,%Proxy_Ftp_User_TextBox%
IniWrite,%ProxyIni%,ProxyInfo,Proxy_Ftp_Password,%Proxy_Ftp_Password_TextBox%
//-
Set,%a%,%ProvideFiles%\aria2.conf
FileCreateBlank,%a%
Set,%w%,%ProvideFiles%\wgetrc
FileCreateBlank,%w%
//---
If,%Proxy_All_Same_CheckBox%,Equal,True,Begin
TxtAddLine,%a%,"all-proxy=%Proxy_TextBox%",Append
//-
TxtAddLine,%w%,"http_proxy=%Proxy_TextBox%",Append
TxtAddLine,%w%,"https_proxy=%Proxy_TextBox%",Append
TxtAddLine,%w%,"ftp_proxy=%Proxy_TextBox%",Append
End
Else,Begin
TxtAddLine,%a%,"http_proxy=%Proxy_Http_TextBox%",Append
TxtAddLine,%a%,"https_proxy=%Proxy_Https_TextBox%",Append
TxtAddLine,%a%,"ftp_proxy=%Proxy_Ftp_TextBox%",Append
//-
TxtAddLine,%w%,"http_proxy=%Proxy_Http_TextBox%",Append
TxtAddLine,%w%,"https_proxy=%Proxy_Https_TextBox%",Append
TxtAddLine,%w%,"ftp_proxy=%Proxy_Ftp_TextBox%",Append
End
//---
If,%UNPW_ALL_CheckBox%,Equal,True,Begin
TxtAddLine,%a%,"all-proxy-user=%UserName_All_TextBox%",Append
TxtAddLine,%a%,"all-proxy-passwd=%Password_All_TextBox%",Append
//-
TxtAddLine,%w%,"proxy-user=%UserName_All_TextBox%",Append
TxtAddLine,%w%,"proxy-password=%Password_All_TextBox%",Append
End
//---
If,%UNPW_Each_CheckBox%,Equal,True,Begin
TxtAddLine,%a%,"http-proxy-user=%Proxy_Http_TextBox%",Append
TxtAddLine,%a%,"http-proxy-passwd=%Proxy_Http_User_TextBox%",Append
TxtAddLine,%a%,"https-proxy-user=%Proxy_Https_User_TextBox%",Append
TxtAddLine,%a%,"https-proxy-passwd=%Proxy_Https_Password_TextBox%",Append
TxtAddLine,%a%,"ftp-proxy-user=%Proxy_Ftp_User_TextBox%",Append
TxtAddLine,%a%,"ftp-proxy-passwd=%Proxy_Ftp_Password_TextBox%",Append
//-
//wget does not support different passwords for different protocols !
//so for now we get only http proxy protocol un/pw for wget
//we can use different pw for https and ftp via Macro Library Wget commandline when required.
TxtAddLine,%w%,"proxy-user=%Proxy_Http_TextBox%",Append
TxtAddLine,%w%,"proxy-password=%Proxy_Http_User_TextBox%",Append
//-
//so better be ready to use different pw for https and ftp via Macro Library Wget commandline when required ;).
TxtAddLine,%w%,"http-proxy-user=%Proxy_Http_TextBox%",Append
TxtAddLine,%w%,"http-proxy-passwd=%Proxy_Http_User_TextBox%",Append
TxtAddLine,%w%,"https-proxy-user=%Proxy_Https_User_TextBox%",Append
TxtAddLine,%w%,"https-proxy-passwd=%Proxy_Https_Password_TextBox%",Append
TxtAddLine,%w%,"ftp-proxy-user=%Proxy_Ftp_User_TextBox%",Append
TxtAddLine,%w%,"ftp-proxy-passwd=%Proxy_Ftp_Password_TextBox%",Append
End
//-
//aria2 true case sensitive here (True wrong)
TxtAddLine,%a%,"http-no-cache=true",Append
//could not get no-cache work with wget config file ->  --no-cache used with Macro Library when Proxy used.
//TxtAddLine,%w%,"no-cache=true",Append
//TxtAddLine,%w%,"no-cache",Append
//-
If,Not,%NoProxy_TextBox%-,Equal,-,Begin
Set,%t%,%NoProxy_TextBox%
StrFormat,Replace,%t%,;,",",%t%
TxtAddLine,%a%,"no_proxy=%t%",Append
End

