Call,TargetDesktop
Set,%w%,%TargetDesktop%\Test_ps1.ps1
FileCreateBlank,%w%
TxtAddLine,%w%,"Get-Disk",Append
TxtAddLine,%w%,"Get-Volume",Append
TxtAddLine,%w%,"pause",Append
End
