[ExecutionPolicy_Config_json]
//Start this section with File Location: %powershell_config_json% (eg. %Target_Prog%\%ProgramFolder%\powershell.config.json )
Set,%w%,%powershell_config_json%
If,ExistFile,%w%,Begin
Retrieve,Md5,%w%,%Before%
//-
//PS 6
TxtReplace,%w%,"#$qMicrosoft.PowerShell:ExecutionPolicy#$q:#$qRemoteSigned#$q","#$qMicrosoft.PowerShell:ExecutionPolicy#$q:#$q%Execution_Policy_ScrollBox%#$q"
//-
//PS 7
TxtReplace,%w%,"#$qMicrosoft.PowerShell:ExecutionPolicy#$q: #$qRemoteSigned#$q","#$qMicrosoft.PowerShell:ExecutionPolicy#$q: #$q%Execution_Policy_ScrollBox%#$q"
//-
Retrieve,Md5,%w%,%After%
If,%Before%,Equal,%After%,Echo,"PowerShell Plugin - Can NOT Set ExecutionPolicy - powershell.config.json NOT CHANGED",Warn
End
Else,Echo,"PowerShell Plugin - Can NOT Set ExecutionPolicy - powershell.config.json NOT FOUND",Warn

