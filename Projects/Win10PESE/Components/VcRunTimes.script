[Variables]
%ContainerFileFolder%=%scriptdir%
%ContainerFileName%=VcRunTimes_File.script
%ContainerFile%=%ContainerFileFolder%\%ContainerFileName%
%FileContainer%=%ContainerFileFolder%\%ContainerFileName%
//-
Extract=Run,%ScriptFile%,PExtract
ExtractCopyApiUcrt=Run,%ScriptFile%,PExtractCopyApiUcrt

[Process]
If,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Echo,"Components PluginCache"
If,Not,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Run,%ScriptFile%,Process_Normal

[Process_Normal]
If,Not,ExistFile,%ContainerFile%,Exit,"'%ScriptTitle%' Container File Missing %ContainerFile%"
StrFormat,RIGHT,%SB_EMB%,1,%EMB%
StrFormat,RIGHT,%SB_VC8%,1,%VC8%
StrFormat,RIGHT,%SB_VC9%,1,%VC9%
StrFormat,RIGHT,%SB_VC10%,1,%VC10%
StrFormat,RIGHT,%SB_VC11%,1,%VC11%
StrFormat,RIGHT,%SB_VC12%,1,%VC12%
StrFormat,RIGHT,%SB_VC14%,1,%VC14%
//- Install VC++ x86 runtime for x86 Build or for x64 Build with Wow64 support.
Set,%x86Runtime%,False
If,%SourceArch%,Equal,x86,Set,%x86Runtime%,True
If,%SourceArch%,Equal,x64,If,%TargetWOW64%,Equal,x86,Set,%x86Runtime%,True
//-
If,ExistDir,%ProjectTemp%\TempExtractFolder\VC++,Shellexecute,Hide,cmd.exe,"/C rd /s /q #$q%ProjectTemp%\TempExtractFolder\VC++#$q"
If,ExistDir,%ProjectTemp%\TempExtractFolder\VC++,Call,DirDelete,%ProjectTemp%\TempExtractFolder\VC++
DirMake,%ProjectTemp%\TempExtractFolder\VC++
// Embedded version (8.0 - 9.0) (0=None, 1=Full)
If,%EMB%,Equal,1,Begin
IniRead,%projectdir%\script.project,IniVariables,SoftwareHive,%SoftwareHive%
IniRead,%projectdir%\script.project,IniVariables,RegistryCache,%RegistryCache%
ExtractFile,%ScriptFile%,Folder,CopySoftKey.cmd,%ProjectTemp%\TempExtractFolder\VC++
Echo,"Installing Embedded Visual C++ 2005 - 2008..."
If,%x86Runtime%,Equal,True,Begin
Call,DirCopy,%Source_Win%\winsxs\x86_microsoft.vc80.crt*,%TargetDir%\Windows\winsxs\,OverWrite
Call,DirCopy,%Source_Win%\winsxs\x86_microsoft.vc90.crt*,%TargetDir%\Windows\winsxs\,OverWrite
FileCopy,%Source_Win%\winsxs\manifests\x86_microsoft.vc80.crt*,%TargetDir%\Windows\winsxs\manifests
FileCopy,%Source_Win%\winsxs\manifests\x86_policy.8.0.microsoft.vc80.crt*,%TargetDir%\Windows\winsxs\manifests
FileCopy,%Source_Win%\winsxs\manifests\x86_microsoft.vc90.crt*,%TargetDir%\Windows\winsxs\manifests
FileCopy,%Source_Win%\winsxs\manifests\x86_policy.9.0.microsoft.vc90.crt*,%TargetDir%\Windows\winsxs\manifests
End
If,%SourceArch%,Equal,x64,Begin
Call,DirCopy,%Source_Win%\winsxs\amd64_microsoft.vc80.crt*,%TargetDir%\Windows\winsxs\,OverWrite
Call,DirCopy,%Source_Win%\winsxs\amd64_microsoft.vc90.crt*,%TargetDir%\Windows\winsxs\,OverWrite
FileCopy,%Source_Win%\winsxs\manifests\amd64_microsoft.vc80.crt*,%TargetDir%\Windows\winsxs\manifests
FileCopy,%Source_Win%\winsxs\manifests\amd64_policy.8.0.microsoft.vc80.crt*,%TargetDir%\Windows\winsxs\manifests
FileCopy,%Source_Win%\winsxs\manifests\amd64_microsoft.vc90.crt*,%TargetDir%\Windows\winsxs\manifests
FileCopy,%Source_Win%\winsxs\manifests\amd64_policy.9.0.microsoft.vc90.crt*,%TargetDir%\Windows\winsxs\manifests
End
End
// Visual C++ 2005 (0=None, 1=Full, 2=Lite)
If,%VC8%,Bigger,0,Begin
Echo,"Installing Visual C++ 2005..."
If,%x86Runtime%,Equal,True,Extract,x86,VC8Lite.7z,%target_win%\WinSxS
If,%SourceArch%,Equal,x64,Extract,x64,VC8Lite.7z,%target_win%\WinSxS
If,%VC8%,Equal,1,Begin
If,%x86Runtime%,Equal,True,Extract,x86,VC8.7z,%target_win%\WinSxS
If,%SourceArch%,Equal,x64,Extract,x64,VC8.7z,%target_win%\WinSxS
End
End
// Visual C++ 2008 (0=None, 1=Full, 2=Lite)
If,%VC9%,Bigger,0,Begin
Echo,"Installing Visual C++ 2008..."
If,%x86Runtime%,Equal,True,Extract,x86,VC9Lite.7z,%target_win%\WinSxS
If,%SourceArch%,Equal,x64,Extract,x64,VC9Lite.7z,%target_win%\WinSxS
If,%VC9%,Equal,1,Begin
If,%x86Runtime%,Equal,True,Extract,x86,VC9.7z,%target_win%\WinSxS
If,%SourceArch%,Equal,x64,Extract,x64,VC9.7z,%target_win%\WinSxS
End
End
// Visual C++ 2010 (0=None, 1=Full)
If,%VC10%,Equal,1,Begin
Echo,"Installing Visual C++ 2010..."
If,%SourceArch%,Equal,x64,Begin
If,%x86Runtime%,Equal,True,Extract,x86,VC10.7z,%target_win%\SysWOW64
Extract,x64,VC10.7z,%target_win%\System32
End
Else,Extract,x86,VC10.7z,%target_win%\System32
End
// Visual C++ 2012 (0=None, 1=Full)
If,%VC11%,Equal,1,Begin
Echo,"Installing Visual C++ 2012..."
If,%SourceArch%,Equal,x64,Begin
If,%x86Runtime%,Equal,True,Extract,x86,VC11.7z,%target_win%\SysWOW64
Extract,x64,VC11.7z,%target_win%\System32
End
Else,Extract,x86,VC11.7z,%target_win%\System32
End
// Visual C++ 2013 (0=None, 1=Full)
If,%VC12%,Equal,1,Begin
Echo,"Installing Visual C++ 2013..."
If,%SourceArch%,Equal,x64,Begin
If,%x86Runtime%,Equal,True,Extract,x86,VC12.7z,%target_win%\SysWOW64
Extract,x64,VC12.7z,%target_win%\System32
End
Else,Extract,x86,VC12.7z,%target_win%\System32
End
// Visual C++ 2017 (0=None, 1=Full)
If,%VC14%,Equal,1,Begin
Echo,"Installing Visual C++ 2017..."
If,%SourceArch%,Equal,x64,Begin
If,%x86Runtime%,Equal,True,Extract,x86,VC14.11.7z,%target_win%\SysWOW64
If,%x86Runtime%,Equal,True,ExtractCopyApiUcrt,x86,VC14.11Ucrt.7z
Extract,x64,VC14.11.7z,%target_win%\System32
ExtractCopyApiUcrt,x64,VC14.11Ucrt.7z
End
Else,Begin
Extract,x86,VC14.11.7z,%target_win%\System32
ExtractCopyApiUcrt,x86,VC14.11Ucrt.7z
End
End
// Register SideBySide for 2005/2008 - 2010-2012 doesn't use crappy SxS :)
If,%x86Runtime%,Equal,True,Run,%ScriptFile%,SxS_x86
If,%SourceArch%,Equal,x64,Run,%ScriptFile%,SxS_x64
//Optional
DirDelete,%ProjectTemp%\TempExtractFolder\VC++,FAST

[PExtract]
// #1Encoded Folder (x86|x64) - #2 Archive (ex: VC14.11.7z) - #3 Target Folder (ex: %target_win%\System32)
ExtractFile,%ContainerFile%,#1,#2,%ProjectTemp%\TempExtractFolder\VC++\#1
ShellExecute,Hide,%Tools%\7z.exe,"x #$q%ProjectTemp%\TempExtractFolder\VC++\#1\#2#$q -y -o#$q#3#$q"

[PExtractCopyApiUcrt]
// #1 Encoded Folder (x86|x64) - #2 Archive (ex: VC14.11Ucrt.7z -> ex ExtractedFolder: ProjectTemp\TempExtractFolder\VC++\x86\VC14.11Ucrt)
StrFormat,RTRIM,#2,3,%t%
Set,%ExtractedFolder%,%ProjectTemp%\TempExtractFolder\VC++\#1\%t%
ExtractFile,%ContainerFile%,#1,#2,%ProjectTemp%\TempExtractFolder\VC++\#1
ShellExecute,Hide,%Tools%\7z.exe,"x #$q%ProjectTemp%\TempExtractFolder\VC++\#1\#2#$q -y -o#$q%ExtractedFolder%#$q"
//-
If,%SourceArch%,Equal,x64,Begin
If,#1,Equal,x86,Set,%TargetCopy%,%target_win%\SysWOW64
If,#1,Equal,x64,Set,%TargetCopy%,%target_win%\System32
End
Else,Set,%TargetCopy%,%target_win%\System32
// Copy ucrtbase.dll in System32 or SysWOW64 without overwrite
If,ExistFile,%ExtractedFolder%\ucrtbase.dll,Begin
If,Not,ExistFile,%TargetCopy%\ucrtbase.dll,FileCopy,%ExtractedFolder%\ucrtbase.dll,%TargetCopy%
End
// Copy api-ms-win*.dll files without overwrite in System32\downlevel or SysWOW64\downlevel folder if exist (Win8.1, Win10) else in System32 or SysWOW64 (Win7, Win8)
// For %F In ("C:\From\*.*") Do If Not Exist "C:\To\%~nxF" Copy "%F" "C:\To\%~nxF"
If,ExistDir,%TargetCopy%\downlevel,Set,%TargetCopy%,%TargetCopy%\downlevel
ShellExecute,Hide,cmd.exe,"/C For #$pF In (#$q%ExtractedFolder%\api-ms-win*.dll#$q) Do If Not Exist #$q%TargetCopy%\#$p~nxF#$q Copy #$q#$pF#$q #$q%TargetCopy%\#$p~nxF#$q"
//DirDelete,%ExtractedFolder%

[SxS_x86]
RegHiveLoad,Tmp_Software,%RegSoftware%
