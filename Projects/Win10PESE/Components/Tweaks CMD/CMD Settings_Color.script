If,%Default_CheckBox%,Equal,True,Run,%ScriptFile%,Process_ColorTable_Result
If,%Default_Systemroot_CheckBox%,Equal,True,Run,%ScriptFile%,Process_ColorTable_Result_SystemRoot
End
//--
Run,%ScriptFile%,Process_BasicColor_Calculate
If,%Default_CheckBox%,Equal,True,Begin
If,%NoWarnROW%,Equal,True,System,ErrorOff,2
RegWrite,HKLM,0x4,Tmp_Default\Console,ScreenColors,%ScreenColorsValue%
RegWrite,HKLM,0x4,Tmp_Default\Console,PopupColors,%PopupColorsValue%
End
If,%Default_Systemroot_CheckBox%,Equal,True,Begin
If,%NoWarnROW%,Equal,True,System,ErrorOff,2
RegWrite,HKLM,0x4,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,ScreenColors,%ScreenColorsValue%
