[Variables]

[Process]
If,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Echo,"Components PluginCache"
If,Not,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Run,%ScriptFile%,Process_Normal

[Process_Normal]
IniRead,%ProjectDir%\script.project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
//--
RegHiveLoad,Tmp_Default,%RegDefault%
//-
//0 (zero) for Let Windows choose what's best for my computer settings.  - Windows will automatically turn on and off the visual effects that it determines will run fine based on your system specs.
//1 for Adjust for best appearance settings. - This will turn on all visual effects.
//2 for Adjust for best Performance settings. - This will turn off all visual effects.
//3 for Custom settings.
//According to ten forums value must be 3 to get UserPreferencesMask values operational on Win8....
If,%NoWarnROW%,Equal,True,System,ErrorOff
If,%VisualEffects_CheckBox%,Equal,True,RegWrite,HKLM,0x4,"Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Explorer\VisualEffects",VisualFXSetting,%VisualEffects_ScrollBox%
//--
//--
//According to TenForums
//VisualEffects=0 --> Hex3=07 -> Bit 16=1 Bit 17=1 Bit 18=1 // Hex5=12 -> Bit32=0 Bit33=0 Bit34=1 Bit34=1 !!!
//VisualEffects=1 --> Hex3=07 -> Bit 16=1 Bit 17=1 Bit 18=1 // Hex5=12 -> Bit32=0 Bit33=0 Bit34=1 Bit34=1 !!!
//VisualEffects=2 --> Hex3=03 -> Bit 16=1 Bit 17=1 Bit 18=0 // Hex5=10 -> Bit32=0 Bit33=1 Bit34=0 Bit34=1 !!!
//--
//--
//--
//--
//--
//--
RegRead,HKLM,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask,%t%
Echo,%t%
//Echo,%t%,Warn
//--
//--
//For Tests
//RegWrite,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask,9E,3C,01,80
//RegWrite,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask,9e,3e,03,80,12,00,00,00
//-
//RegWrite,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask,9E,3C,01,80-->9E,3C,07,80
//01 -> 03 -> Bit 17 // //01 -> 07 -> Bit 18
//-
//Call,RegWriteBinaryBit,HexNo,BitNo,NewBit,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//--
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit0%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,1,0,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit1%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,1,1,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit2%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,1,2,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit3%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,1,3,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit4%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,1,4,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit5%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,1,5,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit6%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,1,6,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit7%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,1,7,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//----
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit8%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,2,0,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit9%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,2,1,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit10%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,2,2,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit11%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,2,3,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit12%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,2,4,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit13%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,2,5,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit14%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,2,6,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit15%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,2,7,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit16%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,3,0,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit17%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,3,1,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit18%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,3,2,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//----
//21/8=8*2 + 5  --> 3rd byte Bit 5
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit21%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,3,5,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//----
//22/8=8*2 + 6  --> 3rd byte Bit 6
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit22%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,3,6,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//----
//31/8=3*8 + 7 --> 4th byte Bit 7
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit31%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,4,7,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//----
If,Not,%OSFamily%,Equal,NT5,Begin
//-----
//38/8=4*8 + 6 --> 5th byte Bit 6
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit38%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,5,6,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-----
//Bit40 --> ImeToolBar -- See Korean_IME plugin (ied206 found for=> NT6.1 )
//40/8=5*8 + 0 --> 6th byte Bit 0
Set,%NewBit%,0
If,%pCheckBox_UPM_Bit40%,Equal,True,Set,%NewBit%,1
Call,RegWriteBinaryBit,6,0,%NewBit%,HKLM,0x3,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask
//-----
End
//---
//----
RegRead,HKLM,"Tmp_Default\Control Panel\Desktop",UserPreferencesMask,%t%
Echo,%t%
//Echo,%t%,Warn
RegHiveUnLoad,Tmp_Default

