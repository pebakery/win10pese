[PCopyDismMuiHost]
// #1=Source Folder - #2=Target Folder
Loop,%ScriptFile%,PCopyDismMuiHost_Loop,1,%NbLang%,#1,#2

[PCopyDismMuiHost_Loop]
StrFormat,Split,%MuiLangAll%,|,#c,#9
If,ExistFile,#1\#9\dism.exe.mui,Begin
FileCopy,#1\#9\*,#2
Loop,Break
End

